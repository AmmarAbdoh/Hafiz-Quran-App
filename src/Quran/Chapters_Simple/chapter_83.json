{
    "verses": [
        {
            "id": 5849,
            "verse_key": "83:1",
            "text_uthmani_simple": "ويل للمطففين"
        },
        {
            "id": 5850,
            "verse_key": "83:2",
            "text_uthmani_simple": "الذين اذا اكتالوا۟ على الناس يستوفون"
        },
        {
            "id": 5851,
            "verse_key": "83:3",
            "text_uthmani_simple": "واذا كالوهم او وزنوهم يخسرون"
        },
        {
            "id": 5852,
            "verse_key": "83:4",
            "text_uthmani_simple": "الا يظن او۟لئك انهم مبعوثون"
        },
        {
            "id": 5853,
            "verse_key": "83:5",
            "text_uthmani_simple": "ليوم عظيم"
        },
        {
            "id": 5854,
            "verse_key": "83:6",
            "text_uthmani_simple": "يوم يقوم الناس لرب العلمين"
        },
        {
            "id": 5855,
            "verse_key": "83:7",
            "text_uthmani_simple": "كلا ان كتب الفجار لفى سجين"
        },
        {
            "id": 5856,
            "verse_key": "83:8",
            "text_uthmani_simple": "وما ادرىك ما سجين"
        },
        {
            "id": 5857,
            "verse_key": "83:9",
            "text_uthmani_simple": "كتب مرقوم"
        },
        {
            "id": 5858,
            "verse_key": "83:10",
            "text_uthmani_simple": "ويل يومئذ للمكذبين"
        },
        {
            "id": 5859,
            "verse_key": "83:11",
            "text_uthmani_simple": "الذين يكذبون بيوم الدين"
        },
        {
            "id": 5860,
            "verse_key": "83:12",
            "text_uthmani_simple": "وما يكذب به الا كل معتد اثيم"
        },
        {
            "id": 5861,
            "verse_key": "83:13",
            "text_uthmani_simple": "اذا تتلى عليه ءايتنا قال اسطير الاولين"
        },
        {
            "id": 5862,
            "verse_key": "83:14",
            "text_uthmani_simple": "كلا بل ران على قلوبهم ما كانوا۟ يكسبون"
        },
        {
            "id": 5863,
            "verse_key": "83:15",
            "text_uthmani_simple": "كلا انهم عن ربهم يومئذ لمحجوبون"
        },
        {
            "id": 5864,
            "verse_key": "83:16",
            "text_uthmani_simple": "ثم انهم لصالوا۟ الجحيم"
        },
        {
            "id": 5865,
            "verse_key": "83:17",
            "text_uthmani_simple": "ثم يقال هذا الذى كنتم به تكذبون"
        },
        {
            "id": 5866,
            "verse_key": "83:18",
            "text_uthmani_simple": "كلا ان كتب الابرار لفى عليين"
        },
        {
            "id": 5867,
            "verse_key": "83:19",
            "text_uthmani_simple": "وما ادرىك ما عليون"
        },
        {
            "id": 5868,
            "verse_key": "83:20",
            "text_uthmani_simple": "كتب مرقوم"
        },
        {
            "id": 5869,
            "verse_key": "83:21",
            "text_uthmani_simple": "يشهده المقربون"
        },
        {
            "id": 5870,
            "verse_key": "83:22",
            "text_uthmani_simple": "ان الابرار لفى نعيم"
        },
        {
            "id": 5871,
            "verse_key": "83:23",
            "text_uthmani_simple": "على الارائك ينظرون"
        },
        {
            "id": 5872,
            "verse_key": "83:24",
            "text_uthmani_simple": "تعرف فى وجوههم نضرة النعيم"
        },
        {
            "id": 5873,
            "verse_key": "83:25",
            "text_uthmani_simple": "يسقون من رحيق مختوم"
        },
        {
            "id": 5874,
            "verse_key": "83:26",
            "text_uthmani_simple": "ختمه مسك وفى ذلك فليتنافس المتنفسون"
        },
        {
            "id": 5875,
            "verse_key": "83:27",
            "text_uthmani_simple": "ومزاجه من تسنيم"
        },
        {
            "id": 5876,
            "verse_key": "83:28",
            "text_uthmani_simple": "عينا يشرب بها المقربون"
        },
        {
            "id": 5877,
            "verse_key": "83:29",
            "text_uthmani_simple": "ان الذين اجرموا۟ كانوا۟ من الذين ءامنوا۟ يضحكون"
        },
        {
            "id": 5878,
            "verse_key": "83:30",
            "text_uthmani_simple": "واذا مروا۟ بهم يتغامزون"
        },
        {
            "id": 5879,
            "verse_key": "83:31",
            "text_uthmani_simple": "واذا انقلبوا۟ الى اهلهم انقلبوا۟ فكهين"
        },
        {
            "id": 5880,
            "verse_key": "83:32",
            "text_uthmani_simple": "واذا راوهم قالوا۟ ان هؤلاء لضالون"
        },
        {
            "id": 5881,
            "verse_key": "83:33",
            "text_uthmani_simple": "وما ارسلوا۟ عليهم حفظين"
        },
        {
            "id": 5882,
            "verse_key": "83:34",
            "text_uthmani_simple": "فاليوم الذين ءامنوا۟ من الكفار يضحكون"
        },
        {
            "id": 5883,
            "verse_key": "83:35",
            "text_uthmani_simple": "على الارائك ينظرون"
        },
        {
            "id": 5884,
            "verse_key": "83:36",
            "text_uthmani_simple": "هل ثوب الكفار ما كانوا۟ يفعلون"
        }
    ],
