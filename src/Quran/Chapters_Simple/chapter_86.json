{
    "verses": [
        {
            "id": 5932,
            "verse_key": "86:1",
            "text_uthmani_simple": "والسماء والطارق"
        },
        {
            "id": 5933,
            "verse_key": "86:2",
            "text_uthmani_simple": "وما ادرىك ما الطارق"
        },
        {
            "id": 5934,
            "verse_key": "86:3",
            "text_uthmani_simple": "النجم الثاقب"
        },
        {
            "id": 5935,
            "verse_key": "86:4",
            "text_uthmani_simple": "ان كل نفس لما عليها حافظ"
        },
        {
            "id": 5936,
            "verse_key": "86:5",
            "text_uthmani_simple": "فلينظر الانسن مم خلق"
        },
        {
            "id": 5937,
            "verse_key": "86:6",
            "text_uthmani_simple": "خلق من ماء دافق"
        },
        {
            "id": 5938,
            "verse_key": "86:7",
            "text_uthmani_simple": "يخرج منۢ بين الصلب والترائب"
        },
        {
            "id": 5939,
            "verse_key": "86:8",
            "text_uthmani_simple": "انه على رجعه لقادر"
        },
        {
            "id": 5940,
            "verse_key": "86:9",
            "text_uthmani_simple": "يوم تبلى السرائر"
        },
        {
            "id": 5941,
            "verse_key": "86:10",
            "text_uthmani_simple": "فما له من قوة ولا ناصر"
        },
        {
            "id": 5942,
            "verse_key": "86:11",
            "text_uthmani_simple": "والسماء ذات الرجع"
        },
        {
            "id": 5943,
            "verse_key": "86:12",
            "text_uthmani_simple": "والارض ذات الصدع"
        },
        {
            "id": 5944,
            "verse_key": "86:13",
            "text_uthmani_simple": "انه لقول فصل"
        },
        {
            "id": 5945,
            "verse_key": "86:14",
            "text_uthmani_simple": "وما هو بالهزل"
        },
        {
            "id": 5946,
            "verse_key": "86:15",
            "text_uthmani_simple": "انهم يكيدون كيدا"
        },
        {
            "id": 5947,
            "verse_key": "86:16",
            "text_uthmani_simple": "واكيد كيدا"
        },
        {
            "id": 5948,
            "verse_key": "86:17",
            "text_uthmani_simple": "فمهل الكفرين امهلهم رويدۢا"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "86"
        }
    }
}