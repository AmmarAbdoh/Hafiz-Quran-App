        },
        {
            "id": 6170,
            "verse_key": "102:2",
            "text_uthmani_simple": "حتى زرتم المقابر"
        },
        {
            "id": 6171,
            "verse_key": "102:3",
            "text_uthmani_simple": "كلا سوف تعلمون"
        },
        {
            "id": 6172,
            "verse_key": "102:4",
            "text_uthmani_simple": "ثم كلا سوف تعلمون"
        },
        {
            "id": 6173,
            "verse_key": "102:5",
