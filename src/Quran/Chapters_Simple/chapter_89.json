{
    "verses": [
        {
            "id": 5994,
            "verse_key": "89:1",
            "text_uthmani_simple": "والفجر"
        },
        {
            "id": 5995,
            "verse_key": "89:2",
            "text_uthmani_simple": "وليال عشر"
        },
        {
            "id": 5996,
            "verse_key": "89:3",
            "text_uthmani_simple": "والشفع والوتر"
        },
        {
            "id": 5997,
            "verse_key": "89:4",
            "text_uthmani_simple": "واليل اذا يسر"
        },
        {
            "id": 5998,
            "verse_key": "89:5",
            "text_uthmani_simple": "هل فى ذلك قسم لذى حجر"
        },
        {
            "id": 5999,
            "verse_key": "89:6",
            "text_uthmani_simple": "الم تر كيف فعل ربك بعاد"
        },
        {
            "id": 6000,
            "verse_key": "89:7",
            "text_uthmani_simple": "ارم ذات العماد"
        },
        {
            "id": 6001,
            "verse_key": "89:8",
            "text_uthmani_simple": "التى لم يخلق مثلها فى البلد"
        },
        {
            "id": 6002,
            "verse_key": "89:9",
            "text_uthmani_simple": "وثمود الذين جابوا۟ الصخر بالواد"
        },
        {
            "id": 6003,
            "verse_key": "89:10",
            "text_uthmani_simple": "وفرعون ذى الاوتاد"
        },
        {
            "id": 6004,
            "verse_key": "89:11",
            "text_uthmani_simple": "الذين طغوا۟ فى البلد"
        },
        {
            "id": 6005,
            "verse_key": "89:12",
            "text_uthmani_simple": "فاكثروا۟ فيها الفساد"
        },
        {
            "id": 6006,
            "verse_key": "89:13",
            "text_uthmani_simple": "فصب عليهم ربك سوط عذاب"
        },
        {
            "id": 6007,
            "verse_key": "89:14",
            "text_uthmani_simple": "ان ربك لبالمرصاد"
        },
        {
            "id": 6008,
            "verse_key": "89:15",
            "text_uthmani_simple": "فاما الانسن اذا ما ابتلىه ربه فاكرمه ونعمه فيقول ربى اكرمن"
        },
        {
            "id": 6009,
            "verse_key": "89:16",
            "text_uthmani_simple": "واما اذا ما ابتلىه فقدر عليه رزقه فيقول ربى اهنن"
        },
        {
            "id": 6010,
            "verse_key": "89:17",
            "text_uthmani_simple": "كلا بل لا تكرمون اليتيم"
        },
        {
            "id": 6011,
            "verse_key": "89:18",
            "text_uthmani_simple": "ولا تحضون على طعام المسكين"
        },
        {
            "id": 6012,
            "verse_key": "89:19",
            "text_uthmani_simple": "وتاكلون التراث اكلا لما"
        },
        {
            "id": 6013,
            "verse_key": "89:20",
            "text_uthmani_simple": "وتحبون المال حبا جما"
        },
        {
            "id": 6014,
            "verse_key": "89:21",
            "text_uthmani_simple": "كلا اذا دكت الارض دكا دكا"
        },
        {
            "id": 6015,
            "verse_key": "89:22",
            "text_uthmani_simple": "وجاء ربك والملك صفا صفا"
        },
        {
            "id": 6016,
            "verse_key": "89:23",
            "text_uthmani_simple": "وجا۟ىء يومئذۭ بجهنم يومئذ يتذكر الانسن وانى له الذكرى"
        },
        {
            "id": 6017,
            "verse_key": "89:24",
            "text_uthmani_simple": "يقول يليتنى قدمت لحياتى"
        },
        {
            "id": 6018,
            "verse_key": "89:25",
            "text_uthmani_simple": "فيومئذ لا يعذب عذابه احد"
        },
        {
            "id": 6019,
            "verse_key": "89:26",
            "text_uthmani_simple": "ولا يوثق وثاقه احد"
        },
        {
            "id": 6020,
            "verse_key": "89:27",
            "text_uthmani_simple": "يايتها النفس المطمئنة"
        },
        {
            "id": 6021,
            "verse_key": "89:28",
            "text_uthmani_simple": "ارجعى الى ربك راضية مرضية"
        },
        {
            "id": 6022,
            "verse_key": "89:29",
            "text_uthmani_simple": "فادخلى فى عبدى"
        },
        {
            "id": 6023,
            "verse_key": "89:30",
            "text_uthmani_simple": "وادخلى جنتى"
        }
    ],
    "meta": {
