{
    "verses": [
        {
            "id": 6158,
            "verse_key": "101:1",
            "text_uthmani_simple": "القارعة"
        },
        {
            "id": 6159,
            "verse_key": "101:2",
            "text_uthmani_simple": "ما القارعة"
        },
        {
            "id": 6160,
            "verse_key": "101:3",
            "text_uthmani_simple": "وما ادرىك ما القارعة"
        },
        {
            "id": 6161,
            "verse_key": "101:4",
            "text_uthmani_simple": "يوم يكون الناس كالفراش المبثوث"
        },
        {
            "id": 6162,
            "verse_key": "101:5",
            "text_uthmani_simple": "وتكون الجبال كالعهن المنفوش"
        },
        {
            "id": 6163,
            "verse_key": "101:6",
            "text_uthmani_simple": "فاما من ثقلت موزينه"
        },
        {
            "id": 6164,
            "verse_key": "101:7",
            "text_uthmani_simple": "فهو فى عيشة راضية"
        },
        {
            "id": 6165,
            "verse_key": "101:8",
            "text_uthmani_simple": "واما من خفت موزينه"
        },
        {
            "id": 6166,
            "verse_key": "101:9",
            "text_uthmani_simple": "فامه هاوية"
        },
        {
            "id": 6167,
            "verse_key": "101:10",
            "text_uthmani_simple": "وما ادرىك ما هيه"
        },
        {
            "id": 6168,
            "verse_key": "101:11",
            "text_uthmani_simple": "نار حاميةۢ"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "101"
        }
    }
}