{
    "verses": [
        {
            "id": 2349,
            "verse_key": "20:1",
            "text_uthmani_simple": "طه"
        },
        {
            "id": 2350,
            "verse_key": "20:2",
            "text_uthmani_simple": "ما انزلنا عليك القرءان لتشقى"
        },
        {
            "id": 2351,
            "verse_key": "20:3",
            "text_uthmani_simple": "الا تذكرة لمن يخشى"
        },
        {
            "id": 2352,
            "verse_key": "20:4",
            "text_uthmani_simple": "تنزيلا ممن خلق الارض والسموت العلى"
        },
        {
            "id": 2353,
            "verse_key": "20:5",
            "text_uthmani_simple": "الرحمن على العرش استوى"
        },
        {
            "id": 2354,
            "verse_key": "20:6",
            "text_uthmani_simple": "له ما فى السموت وما فى الارض وما بينهما وما تحت الثرى"
        },
        {
            "id": 2355,
            "verse_key": "20:7",
            "text_uthmani_simple": "وان تجهر بالقول فانه يعلم السر واخفى"
        },
        {
            "id": 2356,
            "verse_key": "20:8",
            "text_uthmani_simple": "الله لا اله الا هو له الاسماء الحسنى"
        },
        {
            "id": 2357,
            "verse_key": "20:9",
            "text_uthmani_simple": "وهل اتىك حديث موسى"
        },
        {
            "id": 2358,
            "verse_key": "20:10",
            "text_uthmani_simple": "اذ رءا نارا فقال لاهله امكثوا۟ انى ءانست نارا لعلى ءاتيكم منها بقبس او اجد على النار هدى"
        },
        {
            "id": 2359,
            "verse_key": "20:11",
            "text_uthmani_simple": "فلما اتىها نودى يموسى"
        },
        {
            "id": 2360,
            "verse_key": "20:12",
            "text_uthmani_simple": "انى انا۠ ربك فاخلع نعليك انك بالواد المقدس طوى"
        },
        {
            "id": 2361,
            "verse_key": "20:13",
            "text_uthmani_simple": "وانا اخترتك فاستمع لما يوحى"
        },
        {
            "id": 2362,
            "verse_key": "20:14",
            "text_uthmani_simple": "اننى انا الله لا اله الا انا۠ فاعبدنى واقم الصلوة لذكرى"
        },
        {
            "id": 2363,
            "verse_key": "20:15",
            "text_uthmani_simple": "ان الساعة ءاتية اكاد اخفيها لتجزى كل نفسۭ بما تسعى"
        },
        {
            "id": 2364,
            "verse_key": "20:16",
            "text_uthmani_simple": "فلا يصدنك عنها من لا يؤمن بها واتبع هوىه فتردى"
        },
        {
            "id": 2365,
            "verse_key": "20:17",
            "text_uthmani_simple": "وما تلك بيمينك يموسى"
        },
        {
            "id": 2366,
            "verse_key": "20:18",
            "text_uthmani_simple": "قال هى عصاى اتوكؤا۟ عليها واهش بها على غنمى ولى فيها مـارب اخرى"
        },
        {
            "id": 2367,
            "verse_key": "20:19",
            "text_uthmani_simple": "قال القها يموسى"
        },
        {
            "id": 2368,
            "verse_key": "20:20",
            "text_uthmani_simple": "فالقىها فاذا هى حية تسعى"
        },
        {
            "id": 2369,
            "verse_key": "20:21",
            "text_uthmani_simple": "قال خذها ولا تخف سنعيدها سيرتها الاولى"
        },
        {
            "id": 2370,
            "verse_key": "20:22",
            "text_uthmani_simple": "واضمم يدك الى جناحك تخرج بيضاء من غير سوء ءاية اخرى"
        },
        {
            "id": 2371,
            "verse_key": "20:23",
            "text_uthmani_simple": "لنريك من ءايتنا الكبرى"
        },
        {
            "id": 2372,
            "verse_key": "20:24",
            "text_uthmani_simple": "اذهب الى فرعون انه طغى"
        },
        {
            "id": 2373,
            "verse_key": "20:25",
            "text_uthmani_simple": "قال رب اشرح لى صدرى"
        },
        {
            "id": 2374,
            "verse_key": "20:26",
            "text_uthmani_simple": "ويسر لى امرى"
        },
        {
            "id": 2375,
            "verse_key": "20:27",
            "text_uthmani_simple": "واحلل عقدة من لسانى"
        },
        {
            "id": 2376,
            "verse_key": "20:28",
            "text_uthmani_simple": "يفقهوا۟ قولى"
        },
        {
            "id": 2377,
            "verse_key": "20:29",
            "text_uthmani_simple": "واجعل لى وزيرا من اهلى"
        },
        {
            "id": 2378,
            "verse_key": "20:30",
            "text_uthmani_simple": "هرون اخى"
        },
        {
            "id": 2379,
            "verse_key": "20:31",
            "text_uthmani_simple": "اشدد به ازرى"
        },
        {
            "id": 2380,
            "verse_key": "20:32",
            "text_uthmani_simple": "واشركه فى امرى"
        },
        {
            "id": 2381,
            "verse_key": "20:33",
            "text_uthmani_simple": "كى نسبحك كثيرا"
        },
        {
            "id": 2382,
            "verse_key": "20:34",
            "text_uthmani_simple": "ونذكرك كثيرا"
        },
        {
            "id": 2383,
            "verse_key": "20:35",
            "text_uthmani_simple": "انك كنت بنا بصيرا"
        },
        {
            "id": 2384,
            "verse_key": "20:36",
            "text_uthmani_simple": "قال قد اوتيت سؤلك يموسى"
        },
        {
            "id": 2385,
            "verse_key": "20:37",
            "text_uthmani_simple": "ولقد مننا عليك مرة اخرى"
        },
        {
            "id": 2386,
            "verse_key": "20:38",
            "text_uthmani_simple": "اذ اوحينا الى امك ما يوحى"
        },
        {
            "id": 2387,
            "verse_key": "20:39",
            "text_uthmani_simple": "ان اقذفيه فى التابوت فاقذفيه فى اليم فليلقه اليم بالساحل ياخذه عدو لى وعدو له والقيت عليك محبة منى ولتصنع على عينى"
        },
        {
            "id": 2388,
            "verse_key": "20:40",
            "text_uthmani_simple": "اذ تمشى اختك فتقول هل ادلكم على من يكفله فرجعنك الى امك كى تقر عينها ولا تحزن وقتلت نفسا فنجينك من الغم وفتنك فتونا فلبثت سنين فى اهل مدين ثم جئت على قدر يموسى"
        },
        {
            "id": 2389,
            "verse_key": "20:41",
            "text_uthmani_simple": "واصطنعتك لنفسى"
        },
        {
            "id": 2390,
            "verse_key": "20:42",
            "text_uthmani_simple": "اذهب انت واخوك بـايتى ولا تنيا فى ذكرى"
        },
        {
            "id": 2391,
            "verse_key": "20:43",
            "text_uthmani_simple": "اذهبا الى فرعون انه طغى"
        },
        {
            "id": 2392,
            "verse_key": "20:44",
            "text_uthmani_simple": "فقولا له قولا لينا لعله يتذكر او يخشى"
        },
        {
            "id": 2393,
            "verse_key": "20:45",
            "text_uthmani_simple": "قالا ربنا اننا نخاف ان يفرط علينا او ان يطغى"
        },
        {
            "id": 2394,
            "verse_key": "20:46",
            "text_uthmani_simple": "قال لا تخافا اننى معكما اسمع وارى"
        },
        {
            "id": 2395,
            "verse_key": "20:47",
            "text_uthmani_simple": "فاتياه فقولا انا رسولا ربك فارسل معنا بنى اسرءيل ولا تعذبهم قد جئنك بـاية من ربك والسلم على من اتبع الهدى"
        },
        {
            "id": 2396,
            "verse_key": "20:48",
            "text_uthmani_simple": "انا قد اوحى الينا ان العذاب على من كذب وتولى"
        },
        {
            "id": 2397,
            "verse_key": "20:49",
            "text_uthmani_simple": "قال فمن ربكما يموسى"
        },
        {
            "id": 2398,
            "verse_key": "20:50",
            "text_uthmani_simple": "قال ربنا الذى اعطى كل شىء خلقه ثم هدى"
        },
        {
            "id": 2399,
            "verse_key": "20:51",
            "text_uthmani_simple": "قال فما بال القرون الاولى"
        },
        {
            "id": 2400,
            "verse_key": "20:52",
            "text_uthmani_simple": "قال علمها عند ربى فى كتب لا يضل ربى ولا ينسى"
        },
        {
            "id": 2401,
            "verse_key": "20:53",
            "text_uthmani_simple": "الذى جعل لكم الارض مهدا وسلك لكم فيها سبلا وانزل من السماء ماء فاخرجنا به ازوجا من نبات شتى"
        },
        {
            "id": 2402,
            "verse_key": "20:54",
            "text_uthmani_simple": "كلوا۟ وارعوا۟ انعمكم ان فى ذلك لءايت لاو۟لى النهى"
        },
        {
            "id": 2403,
            "verse_key": "20:55",
            "text_uthmani_simple": "منها خلقنكم وفيها نعيدكم ومنها نخرجكم تارة اخرى"
        },
        {
            "id": 2404,
            "verse_key": "20:56",
            "text_uthmani_simple": "ولقد ارينه ءايتنا كلها فكذب وابى"
        },
        {
            "id": 2405,
            "verse_key": "20:57",
            "text_uthmani_simple": "قال اجئتنا لتخرجنا من ارضنا بسحرك يموسى"
        },
        {
            "id": 2406,
            "verse_key": "20:58",
            "text_uthmani_simple": "فلناتينك بسحر مثله فاجعل بيننا وبينك موعدا لا نخلفه نحن ولا انت مكانا سوى"
        },
        {
            "id": 2407,
            "verse_key": "20:59",
            "text_uthmani_simple": "قال موعدكم يوم الزينة وان يحشر الناس ضحى"
        },
        {
            "id": 2408,
            "verse_key": "20:60",
            "text_uthmani_simple": "فتولى فرعون فجمع كيده ثم اتى"
        },
        {
            "id": 2409,
            "verse_key": "20:61",
            "text_uthmani_simple": "قال لهم موسى ويلكم لا تفتروا۟ على الله كذبا فيسحتكم بعذاب وقد خاب من افترى"
        },
        {
            "id": 2410,
            "verse_key": "20:62",
            "text_uthmani_simple": "فتنزعوا۟ امرهم بينهم واسروا۟ النجوى"
        },
        {
            "id": 2411,
            "verse_key": "20:63",
            "text_uthmani_simple": "قالوا۟ ان هذن لسحرن يريدان ان يخرجاكم من ارضكم بسحرهما ويذهبا بطريقتكم المثلى"
        },
        {
            "id": 2412,
            "verse_key": "20:64",
            "text_uthmani_simple": "فاجمعوا۟ كيدكم ثم ائتوا۟ صفا وقد افلح اليوم من استعلى"
        },
        {
            "id": 2413,
            "verse_key": "20:65",
            "text_uthmani_simple": "قالوا۟ يموسى اما ان تلقى واما ان نكون اول من القى"
        },
        {
            "id": 2414,
            "verse_key": "20:66",
            "text_uthmani_simple": "قال بل القوا۟ فاذا حبالهم وعصيهم يخيل اليه من سحرهم انها تسعى"
        },
        {
            "id": 2415,
            "verse_key": "20:67",
            "text_uthmani_simple": "فاوجس فى نفسه خيفة موسى"
        },
        {
            "id": 2416,
            "verse_key": "20:68",
            "text_uthmani_simple": "قلنا لا تخف انك انت الاعلى"
        },
        {
            "id": 2417,
            "verse_key": "20:69",
            "text_uthmani_simple": "والق ما فى يمينك تلقف ما صنعوا۟ انما صنعوا۟ كيد سحر ولا يفلح الساحر حيث اتى"
        },
        {
            "id": 2418,
            "verse_key": "20:70",
            "text_uthmani_simple": "فالقى السحرة سجدا قالوا۟ ءامنا برب هرون وموسى"
        },
        {
            "id": 2419,
            "verse_key": "20:71",
            "text_uthmani_simple": "قال ءامنتم له قبل ان ءاذن لكم انه لكبيركم الذى علمكم السحر فلاقطعن ايديكم وارجلكم من خلف ولاصلبنكم فى جذوع النخل ولتعلمن اينا اشد عذابا وابقى"
        },
        {
            "id": 2420,
            "verse_key": "20:72",
            "text_uthmani_simple": "قالوا۟ لن نؤثرك على ما جاءنا من البينت والذى فطرنا فاقض ما انت قاض انما تقضى هذه الحيوة الدنيا"
        },
        {
            "id": 2421,
            "verse_key": "20:73",
            "text_uthmani_simple": "انا ءامنا بربنا ليغفر لنا خطينا وما اكرهتنا عليه من السحر والله خير وابقى"
        },
        {
            "id": 2422,
            "verse_key": "20:74",
            "text_uthmani_simple": "انه من يات ربه مجرما فان له جهنم لا يموت فيها ولا يحيى"
        },
        {
            "id": 2423,
            "verse_key": "20:75",
            "text_uthmani_simple": "ومن ياته مؤمنا قد عمل الصلحت فاو۟لئك لهم الدرجت العلى"
        },
        {
            "id": 2424,
            "verse_key": "20:76",
            "text_uthmani_simple": "جنت عدن تجرى من تحتها الانهر خلدين فيها وذلك جزاء من تزكى"
        },
        {
            "id": 2425,
            "verse_key": "20:77",
            "text_uthmani_simple": "ولقد اوحينا الى موسى ان اسر بعبادى فاضرب لهم طريقا فى البحر يبسا لا تخف دركا ولا تخشى"
        },
        {
            "id": 2426,
            "verse_key": "20:78",
            "text_uthmani_simple": "فاتبعهم فرعون بجنوده فغشيهم من اليم ما غشيهم"
        },
        {
            "id": 2427,
            "verse_key": "20:79",
            "text_uthmani_simple": "واضل فرعون قومه وما هدى"
        },
        {
            "id": 2428,
            "verse_key": "20:80",
            "text_uthmani_simple": "يبنى اسرءيل قد انجينكم من عدوكم ووعدنكم جانب الطور الايمن ونزلنا عليكم المن والسلوى"
        },
        {
            "id": 2429,
            "verse_key": "20:81",
            "text_uthmani_simple": "كلوا۟ من طيبت ما رزقنكم ولا تطغوا۟ فيه فيحل عليكم غضبى ومن يحلل عليه غضبى فقد هوى"
        },
        {
            "id": 2430,
            "verse_key": "20:82",
            "text_uthmani_simple": "وانى لغفار لمن تاب وءامن وعمل صلحا ثم اهتدى"
        },
        {
            "id": 2431,
            "verse_key": "20:83",
            "text_uthmani_simple": "وما اعجلك عن قومك يموسى"
        },
        {
            "id": 2432,
            "verse_key": "20:84",
            "text_uthmani_simple": "قال هم او۟لاء على اثرى وعجلت اليك رب لترضى"
        },
        {
            "id": 2433,
            "verse_key": "20:85",
            "text_uthmani_simple": "قال فانا قد فتنا قومك منۢ بعدك واضلهم السامرى"
        },
        {
            "id": 2434,
            "verse_key": "20:86",
            "text_uthmani_simple": "فرجع موسى الى قومه غضبن اسفا قال يقوم الم يعدكم ربكم وعدا حسنا افطال عليكم العهد ام اردتم ان يحل عليكم غضب من ربكم فاخلفتم موعدى"
        },
        {
            "id": 2435,
            "verse_key": "20:87",
            "text_uthmani_simple": "قالوا۟ ما اخلفنا موعدك بملكنا ولكنا حملنا اوزارا من زينة القوم فقذفنها فكذلك القى السامرى"
        },
        {
            "id": 2436,
            "verse_key": "20:88",
            "text_uthmani_simple": "فاخرج لهم عجلا جسدا له خوار فقالوا۟ هذا الهكم واله موسى فنسى"
        },
        {
            "id": 2437,
            "verse_key": "20:89",
            "text_uthmani_simple": "افلا يرون الا يرجع اليهم قولا ولا يملك لهم ضرا ولا نفعا"
        },
        {
            "id": 2438,
            "verse_key": "20:90",
            "text_uthmani_simple": "ولقد قال لهم هرون من قبل يقوم انما فتنتم به وان ربكم الرحمن فاتبعونى واطيعوا۟ امرى"
        },
        {
            "id": 2439,
            "verse_key": "20:91",
            "text_uthmani_simple": "قالوا۟ لن نبرح عليه عكفين حتى يرجع الينا موسى"
        },
        {
            "id": 2440,
            "verse_key": "20:92",
            "text_uthmani_simple": "قال يهرون ما منعك اذ رايتهم ضلوا۟"
        },
        {
            "id": 2441,
            "verse_key": "20:93",
            "text_uthmani_simple": "الا تتبعن افعصيت امرى"
        },
        {
            "id": 2442,
            "verse_key": "20:94",
            "text_uthmani_simple": "قال يبنؤم لا تاخذ بلحيتى ولا براسى انى خشيت ان تقول فرقت بين بنى اسرءيل ولم ترقب قولى"
        },
        {
            "id": 2443,
            "verse_key": "20:95",
            "text_uthmani_simple": "قال فما خطبك يسمرى"
        },
        {
            "id": 2444,
            "verse_key": "20:96",
            "text_uthmani_simple": "قال بصرت بما لم يبصروا۟ به فقبضت قبضة من اثر الرسول فنبذتها وكذلك سولت لى نفسى"
        },
        {
            "id": 2445,
            "verse_key": "20:97",
            "text_uthmani_simple": "قال فاذهب فان لك فى الحيوة ان تقول لا مساس وان لك موعدا لن تخلفه وانظر الى الهك الذى ظلت عليه عاكفا لنحرقنه ثم لننسفنه فى اليم نسفا"
        },
        {
            "id": 2446,
            "verse_key": "20:98",
            "text_uthmani_simple": "انما الهكم الله الذى لا اله الا هو وسع كل شىء علما"
        },
        {
            "id": 2447,
            "verse_key": "20:99",
            "text_uthmani_simple": "كذلك نقص عليك من انۢباء ما قد سبق وقد ءاتينك من لدنا ذكرا"
        },
        {
            "id": 2448,
            "verse_key": "20:100",
            "text_uthmani_simple": "من اعرض عنه فانه يحمل يوم القيمة وزرا"
        },
        {
            "id": 2449,
            "verse_key": "20:101",
            "text_uthmani_simple": "خلدين فيه وساء لهم يوم القيمة حملا"
        },
        {
            "id": 2450,
            "verse_key": "20:102",
            "text_uthmani_simple": "يوم ينفخ فى الصور ونحشر المجرمين يومئذ زرقا"
        },
        {
            "id": 2451,
            "verse_key": "20:103",
            "text_uthmani_simple": "يتخفتون بينهم ان لبثتم الا عشرا"
        },
        {
            "id": 2452,
            "verse_key": "20:104",
            "text_uthmani_simple": "نحن اعلم بما يقولون اذ يقول امثلهم طريقة ان لبثتم الا يوما"
        },
        {
            "id": 2453,
            "verse_key": "20:105",
            "text_uthmani_simple": "ويسـلونك عن الجبال فقل ينسفها ربى نسفا"
        },
        {
            "id": 2454,
            "verse_key": "20:106",
            "text_uthmani_simple": "فيذرها قاعا صفصفا"
        },
        {
            "id": 2455,
            "verse_key": "20:107",
            "text_uthmani_simple": "لا ترى فيها عوجا ولا امتا"
        },
        {
            "id": 2456,
            "verse_key": "20:108",
            "text_uthmani_simple": "يومئذ يتبعون الداعى لا عوج له وخشعت الاصوات للرحمن فلا تسمع الا همسا"
        },
        {
            "id": 2457,
            "verse_key": "20:109",
            "text_uthmani_simple": "يومئذ لا تنفع الشفعة الا من اذن له الرحمن ورضى له قولا"
        },
        {
            "id": 2458,
            "verse_key": "20:110",
            "text_uthmani_simple": "يعلم ما بين ايديهم وما خلفهم ولا يحيطون به علما"
        },
        {
            "id": 2459,
            "verse_key": "20:111",
            "text_uthmani_simple": "وعنت الوجوه للحى القيوم وقد خاب من حمل ظلما"
        },
        {
            "id": 2460,
            "verse_key": "20:112",
            "text_uthmani_simple": "ومن يعمل من الصلحت وهو مؤمن فلا يخاف ظلما ولا هضما"
        },
        {
            "id": 2461,
            "verse_key": "20:113",
            "text_uthmani_simple": "وكذلك انزلنه قرءانا عربيا وصرفنا فيه من الوعيد لعلهم يتقون او يحدث لهم ذكرا"
        },
        {
            "id": 2462,
            "verse_key": "20:114",
            "text_uthmani_simple": "فتعلى الله الملك الحق ولا تعجل بالقرءان من قبل ان يقضى اليك وحيه وقل رب زدنى علما"
        },
        {
            "id": 2463,
            "verse_key": "20:115",
            "text_uthmani_simple": "ولقد عهدنا الى ءادم من قبل فنسى ولم نجد له عزما"
        },
        {
            "id": 2464,
            "verse_key": "20:116",
            "text_uthmani_simple": "واذ قلنا للملئكة اسجدوا۟ لءادم فسجدوا۟ الا ابليس ابى"
        },
        {
            "id": 2465,
            "verse_key": "20:117",
            "text_uthmani_simple": "فقلنا يـادم ان هذا عدو لك ولزوجك فلا يخرجنكما من الجنة فتشقى"
        },
        {
            "id": 2466,
            "verse_key": "20:118",
            "text_uthmani_simple": "ان لك الا تجوع فيها ولا تعرى"
        },
        {
            "id": 2467,
            "verse_key": "20:119",
            "text_uthmani_simple": "وانك لا تظمؤا۟ فيها ولا تضحى"
        },
        {
            "id": 2468,
            "verse_key": "20:120",
            "text_uthmani_simple": "فوسوس اليه الشيطن قال يـادم هل ادلك على شجرة الخلد وملك لا يبلى"
        },
        {
            "id": 2469,
            "verse_key": "20:121",
            "text_uthmani_simple": "فاكلا منها فبدت لهما سوءتهما وطفقا يخصفان عليهما من ورق الجنة وعصى ءادم ربه فغوى"
        },
        {
            "id": 2470,
            "verse_key": "20:122",
            "text_uthmani_simple": "ثم اجتبه ربه فتاب عليه وهدى"
        },
        {
            "id": 2471,
            "verse_key": "20:123",
            "text_uthmani_simple": "قال اهبطا منها جميعۢا بعضكم لبعض عدو فاما ياتينكم منى هدى فمن اتبع هداى فلا يضل ولا يشقى"
        },
        {
            "id": 2472,
            "verse_key": "20:124",
            "text_uthmani_simple": "ومن اعرض عن ذكرى فان له معيشة ضنكا ونحشره يوم القيمة اعمى"
        },
        {
            "id": 2473,
            "verse_key": "20:125",
            "text_uthmani_simple": "قال رب لم حشرتنى اعمى وقد كنت بصيرا"
        },
        {
            "id": 2474,
            "verse_key": "20:126",
            "text_uthmani_simple": "قال كذلك اتتك ءايتنا فنسيتها وكذلك اليوم تنسى"
        },
        {
            "id": 2475,
            "verse_key": "20:127",
            "text_uthmani_simple": "وكذلك نجزى من اسرف ولم يؤمنۢ بـايت ربه ولعذاب الءاخرة اشد وابقى"
        },
        {
            "id": 2476,
            "verse_key": "20:128",
            "text_uthmani_simple": "افلم يهد لهم كم اهلكنا قبلهم من القرون يمشون فى مسكنهم ان فى ذلك لءايت لاو۟لى النهى"
        },
        {
            "id": 2477,
            "verse_key": "20:129",
            "text_uthmani_simple": "ولولا كلمة سبقت من ربك لكان لزاما واجل مسمى"
        },
        {
            "id": 2478,
            "verse_key": "20:130",
            "text_uthmani_simple": "فاصبر على ما يقولون وسبح بحمد ربك قبل طلوع الشمس وقبل غروبها ومن ءانائ اليل فسبح واطراف النهار لعلك ترضى"
        },
        {
            "id": 2479,
            "verse_key": "20:131",
            "text_uthmani_simple": "ولا تمدن عينيك الى ما متعنا به ازوجا منهم زهرة الحيوة الدنيا لنفتنهم فيه ورزق ربك خير وابقى"
        },
        {
            "id": 2480,
            "verse_key": "20:132",
            "text_uthmani_simple": "وامر اهلك بالصلوة واصطبر عليها لا نسـلك رزقا نحن نرزقك والعقبة للتقوى"
        },
        {
            "id": 2481,
            "verse_key": "20:133",
            "text_uthmani_simple": "وقالوا۟ لولا ياتينا بـاية من ربه اولم تاتهم بينة ما فى الصحف الاولى"
        },
        {
            "id": 2482,
            "verse_key": "20:134",
            "text_uthmani_simple": "ولو انا اهلكنهم بعذاب من قبله لقالوا۟ ربنا لولا ارسلت الينا رسولا فنتبع ءايتك من قبل ان نذل ونخزى"
        },
        {
            "id": 2483,
            "verse_key": "20:135",
            "text_uthmani_simple": "قل كل متربص فتربصوا۟ فستعلمون من اصحب الصرط السوى ومن اهتدى"
        }
    ],
