{
    "verses": [
        {
            "id": 5272,
            "verse_key": "68:1",
            "text_uthmani_simple": "ن والقلم وما يسطرون"
        },
        {
            "id": 5273,
            "verse_key": "68:2",
            "text_uthmani_simple": "ما انت بنعمة ربك بمجنون"
        },
        {
            "id": 5274,
            "verse_key": "68:3",
            "text_uthmani_simple": "وان لك لاجرا غير ممنون"
        },
        {
            "id": 5275,
            "verse_key": "68:4",
            "text_uthmani_simple": "وانك لعلى خلق عظيم"
        },
        {
            "id": 5276,
            "verse_key": "68:5",
            "text_uthmani_simple": "فستبصر ويبصرون"
        },
        {
            "id": 5277,
            "verse_key": "68:6",
            "text_uthmani_simple": "باييكم المفتون"
        },
        {
            "id": 5278,
            "verse_key": "68:7",
            "text_uthmani_simple": "ان ربك هو اعلم بمن ضل عن سبيله وهو اعلم بالمهتدين"
        },
        {
            "id": 5279,
            "verse_key": "68:8",
            "text_uthmani_simple": "فلا تطع المكذبين"
        },
        {
            "id": 5280,
            "verse_key": "68:9",
            "text_uthmani_simple": "ودوا۟ لو تدهن فيدهنون"
        },
        {
            "id": 5281,
            "verse_key": "68:10",
            "text_uthmani_simple": "ولا تطع كل حلاف مهين"
        },
        {
            "id": 5282,
            "verse_key": "68:11",
            "text_uthmani_simple": "هماز مشاءۭ بنميم"
        },
        {
            "id": 5283,
            "verse_key": "68:12",
            "text_uthmani_simple": "مناع للخير معتد اثيم"
        },
        {
            "id": 5284,
            "verse_key": "68:13",
            "text_uthmani_simple": "عتلۭ بعد ذلك زنيم"
        },
        {
            "id": 5285,
            "verse_key": "68:14",
            "text_uthmani_simple": "ان كان ذا مال وبنين"
        },
        {
            "id": 5286,
            "verse_key": "68:15",
            "text_uthmani_simple": "اذا تتلى عليه ءايتنا قال اسطير الاولين"
        },
        {
            "id": 5287,
            "verse_key": "68:16",
            "text_uthmani_simple": "سنسمه على الخرطوم"
        },
        {
            "id": 5288,
            "verse_key": "68:17",
            "text_uthmani_simple": "انا بلونهم كما بلونا اصحب الجنة اذ اقسموا۟ ليصرمنها مصبحين"
        },
        {
            "id": 5289,
            "verse_key": "68:18",
            "text_uthmani_simple": "ولا يستثنون"
        },
        {
            "id": 5290,
            "verse_key": "68:19",
            "text_uthmani_simple": "فطاف عليها طائف من ربك وهم نائمون"
        },
        {
            "id": 5291,
            "verse_key": "68:20",
            "text_uthmani_simple": "فاصبحت كالصريم"
        },
        {
            "id": 5292,
            "verse_key": "68:21",
            "text_uthmani_simple": "فتنادوا۟ مصبحين"
        },
        {
            "id": 5293,
            "verse_key": "68:22",
            "text_uthmani_simple": "ان اغدوا۟ على حرثكم ان كنتم صرمين"
        },
        {
            "id": 5294,
            "verse_key": "68:23",
            "text_uthmani_simple": "فانطلقوا۟ وهم يتخفتون"
        },
        {
            "id": 5295,
            "verse_key": "68:24",
            "text_uthmani_simple": "ان لا يدخلنها اليوم عليكم مسكين"
        },
        {
            "id": 5296,
            "verse_key": "68:25",
            "text_uthmani_simple": "وغدوا۟ على حرد قدرين"
        },
        {
            "id": 5297,
            "verse_key": "68:26",
            "text_uthmani_simple": "فلما راوها قالوا۟ انا لضالون"
        },
        {
            "id": 5298,
            "verse_key": "68:27",
            "text_uthmani_simple": "بل نحن محرومون"
        },
        {
            "id": 5299,
            "verse_key": "68:28",
            "text_uthmani_simple": "قال اوسطهم الم اقل لكم لولا تسبحون"
        },
        {
            "id": 5300,
            "verse_key": "68:29",
            "text_uthmani_simple": "قالوا۟ سبحن ربنا انا كنا ظلمين"
        },
        {
            "id": 5301,
            "verse_key": "68:30",
            "text_uthmani_simple": "فاقبل بعضهم على بعض يتلومون"
        },
        {
            "id": 5302,
            "verse_key": "68:31",
            "text_uthmani_simple": "قالوا۟ يويلنا انا كنا طغين"
        },
        {
            "id": 5303,
            "verse_key": "68:32",
            "text_uthmani_simple": "عسى ربنا ان يبدلنا خيرا منها انا الى ربنا رغبون"
        },
        {
            "id": 5304,
            "verse_key": "68:33",
            "text_uthmani_simple": "كذلك العذاب ولعذاب الءاخرة اكبر لو كانوا۟ يعلمون"
        },
        {
            "id": 5305,
            "verse_key": "68:34",
            "text_uthmani_simple": "ان للمتقين عند ربهم جنت النعيم"
        },
        {
            "id": 5306,
            "verse_key": "68:35",
            "text_uthmani_simple": "افنجعل المسلمين كالمجرمين"
        },
        {
            "id": 5307,
            "verse_key": "68:36",
            "text_uthmani_simple": "ما لكم كيف تحكمون"
        },
        {
            "id": 5308,
            "verse_key": "68:37",
            "text_uthmani_simple": "ام لكم كتب فيه تدرسون"
        },
        {
            "id": 5309,
            "verse_key": "68:38",
            "text_uthmani_simple": "ان لكم فيه لما تخيرون"
        },
        {
            "id": 5310,
            "verse_key": "68:39",
            "text_uthmani_simple": "ام لكم ايمن علينا بلغة الى يوم القيمة ان لكم لما تحكمون"
        },
        {
            "id": 5311,
            "verse_key": "68:40",
            "text_uthmani_simple": "سلهم ايهم بذلك زعيم"
        },
        {
            "id": 5312,
            "verse_key": "68:41",
            "text_uthmani_simple": "ام لهم شركاء فلياتوا۟ بشركائهم ان كانوا۟ صدقين"
        },
        {
            "id": 5313,
            "verse_key": "68:42",
            "text_uthmani_simple": "يوم يكشف عن ساق ويدعون الى السجود فلا يستطيعون"
        },
        {
            "id": 5314,
            "verse_key": "68:43",
            "text_uthmani_simple": "خشعة ابصرهم ترهقهم ذلة وقد كانوا۟ يدعون الى السجود وهم سلمون"
        },
        {
            "id": 5315,
            "verse_key": "68:44",
            "text_uthmani_simple": "فذرنى ومن يكذب بهذا الحديث سنستدرجهم من حيث لا يعلمون"
        },
        {
            "id": 5316,
            "verse_key": "68:45",
            "text_uthmani_simple": "واملى لهم ان كيدى متين"
        },
        {
            "id": 5317,
            "verse_key": "68:46",
            "text_uthmani_simple": "ام تسـلهم اجرا فهم من مغرم مثقلون"
        },
        {
            "id": 5318,
            "verse_key": "68:47",
            "text_uthmani_simple": "ام عندهم الغيب فهم يكتبون"
        },
        {
            "id": 5319,
            "verse_key": "68:48",
            "text_uthmani_simple": "فاصبر لحكم ربك ولا تكن كصاحب الحوت اذ نادى وهو مكظوم"
        },
        {
            "id": 5320,
            "verse_key": "68:49",
            "text_uthmani_simple": "لولا ان تدركه نعمة من ربه لنبذ بالعراء وهو مذموم"
        },
        {
            "id": 5321,
            "verse_key": "68:50",
            "text_uthmani_simple": "فاجتبه ربه فجعله من الصلحين"
        },
        {
            "id": 5322,
            "verse_key": "68:51",
            "text_uthmani_simple": "وان يكاد الذين كفروا۟ ليزلقونك بابصرهم لما سمعوا۟ الذكر ويقولون انه لمجنون"
        },
        {
            "id": 5323,
            "verse_key": "68:52",
            "text_uthmani_simple": "وما هو الا ذكر للعلمين"
        }
    ],
