{
    "verses": [
        {
            "id": 5376,
            "verse_key": "70:1",
            "text_uthmani_simple": "سال سائلۢ بعذاب واقع"
        },
        {
            "id": 5377,
            "verse_key": "70:2",
            "text_uthmani_simple": "للكفرين ليس له دافع"
        },
        {
            "id": 5378,
            "verse_key": "70:3",
            "text_uthmani_simple": "من الله ذى المعارج"
        },
        {
            "id": 5379,
            "verse_key": "70:4",
            "text_uthmani_simple": "تعرج الملئكة والروح اليه فى يوم كان مقداره خمسين الف سنة"
        },
        {
            "id": 5380,
            "verse_key": "70:5",
            "text_uthmani_simple": "فاصبر صبرا جميلا"
        },
        {
            "id": 5381,
            "verse_key": "70:6",
            "text_uthmani_simple": "انهم يرونه بعيدا"
        },
        {
            "id": 5382,
            "verse_key": "70:7",
            "text_uthmani_simple": "ونرىه قريبا"
        },
        {
            "id": 5383,
            "verse_key": "70:8",
            "text_uthmani_simple": "يوم تكون السماء كالمهل"
        },
        {
            "id": 5384,
            "verse_key": "70:9",
            "text_uthmani_simple": "وتكون الجبال كالعهن"
        },
        {
            "id": 5385,
            "verse_key": "70:10",
            "text_uthmani_simple": "ولا يسـل حميم حميما"
        },
        {
            "id": 5386,
            "verse_key": "70:11",
            "text_uthmani_simple": "يبصرونهم يود المجرم لو يفتدى من عذاب يومئذۭ ببنيه"
        },
        {
            "id": 5387,
            "verse_key": "70:12",
            "text_uthmani_simple": "وصحبته واخيه"
        },
        {
            "id": 5388,
            "verse_key": "70:13",
            "text_uthmani_simple": "وفصيلته التى تـويه"
        },
        {
            "id": 5389,
            "verse_key": "70:14",
            "text_uthmani_simple": "ومن فى الارض جميعا ثم ينجيه"
        },
        {
            "id": 5390,
            "verse_key": "70:15",
            "text_uthmani_simple": "كلا انها لظى"
        },
        {
            "id": 5391,
            "verse_key": "70:16",
            "text_uthmani_simple": "نزاعة للشوى"
        },
        {
            "id": 5392,
            "verse_key": "70:17",
            "text_uthmani_simple": "تدعوا۟ من ادبر وتولى"
        },
        {
            "id": 5393,
            "verse_key": "70:18",
            "text_uthmani_simple": "وجمع فاوعى"
        },
        {
            "id": 5394,
            "verse_key": "70:19",
            "text_uthmani_simple": "ان الانسن خلق هلوعا"
        },
        {
            "id": 5395,
            "verse_key": "70:20",
            "text_uthmani_simple": "اذا مسه الشر جزوعا"
        },
        {
            "id": 5396,
            "verse_key": "70:21",
            "text_uthmani_simple": "واذا مسه الخير منوعا"
        },
        {
            "id": 5397,
            "verse_key": "70:22",
            "text_uthmani_simple": "الا المصلين"
        },
        {
            "id": 5398,
            "verse_key": "70:23",
            "text_uthmani_simple": "الذين هم على صلاتهم دائمون"
        },
        {
            "id": 5399,
            "verse_key": "70:24",
            "text_uthmani_simple": "والذين فى امولهم حق معلوم"
        },
        {
            "id": 5400,
            "verse_key": "70:25",
            "text_uthmani_simple": "للسائل والمحروم"
        },
        {
            "id": 5401,
            "verse_key": "70:26",
            "text_uthmani_simple": "والذين يصدقون بيوم الدين"
        },
        {
            "id": 5402,
            "verse_key": "70:27",
            "text_uthmani_simple": "والذين هم من عذاب ربهم مشفقون"
        },
        {
            "id": 5403,
            "verse_key": "70:28",
            "text_uthmani_simple": "ان عذاب ربهم غير مامون"
        },
        {
            "id": 5404,
            "verse_key": "70:29",
            "text_uthmani_simple": "والذين هم لفروجهم حفظون"
        },
        {
            "id": 5405,
            "verse_key": "70:30",
            "text_uthmani_simple": "الا على ازوجهم او ما ملكت ايمنهم فانهم غير ملومين"
        },
        {
            "id": 5406,
            "verse_key": "70:31",
            "text_uthmani_simple": "فمن ابتغى وراء ذلك فاو۟لئك هم العادون"
        },
        {
            "id": 5407,
            "verse_key": "70:32",
            "text_uthmani_simple": "والذين هم لامنتهم وعهدهم رعون"
        },
        {
            "id": 5408,
            "verse_key": "70:33",
            "text_uthmani_simple": "والذين هم بشهدتهم قائمون"
        },
        {
            "id": 5409,
            "verse_key": "70:34",
            "text_uthmani_simple": "والذين هم على صلاتهم يحافظون"
        },
        {
            "id": 5410,
            "verse_key": "70:35",
            "text_uthmani_simple": "او۟لئك فى جنت مكرمون"
        },
        {
            "id": 5411,
            "verse_key": "70:36",
            "text_uthmani_simple": "فمال الذين كفروا۟ قبلك مهطعين"
        },
        {
            "id": 5412,
            "verse_key": "70:37",
            "text_uthmani_simple": "عن اليمين وعن الشمال عزين"
        },
        {
            "id": 5413,
            "verse_key": "70:38",
            "text_uthmani_simple": "ايطمع كل امرئ منهم ان يدخل جنة نعيم"
        },
        {
            "id": 5414,
            "verse_key": "70:39",
            "text_uthmani_simple": "كلا انا خلقنهم مما يعلمون"
        },
        {
            "id": 5415,
            "verse_key": "70:40",
            "text_uthmani_simple": "فلا اقسم برب المشرق والمغرب انا لقدرون"
        },
        {
            "id": 5416,
            "verse_key": "70:41",
            "text_uthmani_simple": "على ان نبدل خيرا منهم وما نحن بمسبوقين"
        },
        {
            "id": 5417,
            "verse_key": "70:42",
            "text_uthmani_simple": "فذرهم يخوضوا۟ ويلعبوا۟ حتى يلقوا۟ يومهم الذى يوعدون"
        },
        {
            "id": 5418,
            "verse_key": "70:43",
            "text_uthmani_simple": "يوم يخرجون من الاجداث سراعا كانهم الى نصب يوفضون"
        },
        {
            "id": 5419,
            "verse_key": "70:44",
            "text_uthmani_simple": "خشعة ابصرهم ترهقهم ذلة ذلك اليوم الذى كانوا۟ يوعدون"
        }
    ],
