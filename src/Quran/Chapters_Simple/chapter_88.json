{
    "verses": [
        {
            "id": 5968,
            "verse_key": "88:1",
            "text_uthmani_simple": "هل اتىك حديث الغشية"
        },
        {
            "id": 5969,
            "verse_key": "88:2",
            "text_uthmani_simple": "وجوه يومئذ خشعة"
        },
        {
            "id": 5970,
            "verse_key": "88:3",
            "text_uthmani_simple": "عاملة ناصبة"
        },
        {
            "id": 5971,
            "verse_key": "88:4",
            "text_uthmani_simple": "تصلى نارا حامية"
        },
        {
            "id": 5972,
            "verse_key": "88:5",
            "text_uthmani_simple": "تسقى من عين ءانية"
        },
        {
            "id": 5973,
            "verse_key": "88:6",
            "text_uthmani_simple": "ليس لهم طعام الا من ضريع"
        },
        {
            "id": 5974,
            "verse_key": "88:7",
            "text_uthmani_simple": "لا يسمن ولا يغنى من جوع"
        },
        {
            "id": 5975,
            "verse_key": "88:8",
            "text_uthmani_simple": "وجوه يومئذ ناعمة"
        },
        {
            "id": 5976,
            "verse_key": "88:9",
            "text_uthmani_simple": "لسعيها راضية"
        },
        {
            "id": 5977,
            "verse_key": "88:10",
            "text_uthmani_simple": "فى جنة عالية"
        },
        {
            "id": 5978,
            "verse_key": "88:11",
            "text_uthmani_simple": "لا تسمع فيها لغية"
        },
        {
            "id": 5979,
            "verse_key": "88:12",
            "text_uthmani_simple": "فيها عين جارية"
        },
        {
            "id": 5980,
            "verse_key": "88:13",
            "text_uthmani_simple": "فيها سرر مرفوعة"
        },
        {
            "id": 5981,
            "verse_key": "88:14",
            "text_uthmani_simple": "واكواب موضوعة"
        },
        {
            "id": 5982,
            "verse_key": "88:15",
            "text_uthmani_simple": "ونمارق مصفوفة"
        },
        {
            "id": 5983,
            "verse_key": "88:16",
            "text_uthmani_simple": "وزرابى مبثوثة"
        },
        {
            "id": 5984,
            "verse_key": "88:17",
            "text_uthmani_simple": "افلا ينظرون الى الابل كيف خلقت"
        },
        {
            "id": 5985,
            "verse_key": "88:18",
            "text_uthmani_simple": "والى السماء كيف رفعت"
        },
        {
            "id": 5986,
            "verse_key": "88:19",
            "text_uthmani_simple": "والى الجبال كيف نصبت"
        },
        {
            "id": 5987,
            "verse_key": "88:20",
            "text_uthmani_simple": "والى الارض كيف سطحت"
        },
        {
            "id": 5988,
            "verse_key": "88:21",
            "text_uthmani_simple": "فذكر انما انت مذكر"
        },
        {
            "id": 5989,
            "verse_key": "88:22",
            "text_uthmani_simple": "لست عليهم بمصيطر"
        },
        {
            "id": 5990,
            "verse_key": "88:23",
            "text_uthmani_simple": "الا من تولى وكفر"
        },
        {
            "id": 5991,
            "verse_key": "88:24",
            "text_uthmani_simple": "فيعذبه الله العذاب الاكبر"
        },
        {
            "id": 5992,
            "verse_key": "88:25",
            "text_uthmani_simple": "ان الينا ايابهم"
        },
        {
            "id": 5993,
            "verse_key": "88:26",
            "text_uthmani_simple": "ثم ان علينا حسابهم"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "88"
        }
    }
}