{
    "verses": [
        {
            "id": 5759,
            "verse_key": "80:1",
            "text_uthmani_simple": "عبس وتولى"
        },
        {
            "id": 5760,
            "verse_key": "80:2",
            "text_uthmani_simple": "ان جاءه الاعمى"
        },
        {
            "id": 5761,
            "verse_key": "80:3",
            "text_uthmani_simple": "وما يدريك لعله يزكى"
        },
        {
            "id": 5762,
            "verse_key": "80:4",
            "text_uthmani_simple": "او يذكر فتنفعه الذكرى"
        },
        {
            "id": 5763,
            "verse_key": "80:5",
            "text_uthmani_simple": "اما من استغنى"
        },
        {
            "id": 5764,
            "verse_key": "80:6",
            "text_uthmani_simple": "فانت له تصدى"
        },
        {
            "id": 5765,
            "verse_key": "80:7",
            "text_uthmani_simple": "وما عليك الا يزكى"
        },
        {
            "id": 5766,
            "verse_key": "80:8",
            "text_uthmani_simple": "واما من جاءك يسعى"
        },
        {
            "id": 5767,
            "verse_key": "80:9",
            "text_uthmani_simple": "وهو يخشى"
        },
        {
            "id": 5768,
            "verse_key": "80:10",
            "text_uthmani_simple": "فانت عنه تلهى"
        },
        {
            "id": 5769,
            "verse_key": "80:11",
            "text_uthmani_simple": "كلا انها تذكرة"
        },
        {
            "id": 5770,
            "verse_key": "80:12",
            "text_uthmani_simple": "فمن شاء ذكره"
        },
        {
            "id": 5771,
            "verse_key": "80:13",
            "text_uthmani_simple": "فى صحف مكرمة"
        },
        {
            "id": 5772,
            "verse_key": "80:14",
            "text_uthmani_simple": "مرفوعة مطهرةۭ"
        },
        {
            "id": 5773,
            "verse_key": "80:15",
            "text_uthmani_simple": "بايدى سفرة"
        },
        {
            "id": 5774,
            "verse_key": "80:16",
            "text_uthmani_simple": "كرامۭ بررة"
        },
        {
            "id": 5775,
            "verse_key": "80:17",
            "text_uthmani_simple": "قتل الانسن ما اكفره"
        },
        {
            "id": 5776,
            "verse_key": "80:18",
            "text_uthmani_simple": "من اى شىء خلقه"
        },
        {
            "id": 5777,
            "verse_key": "80:19",
            "text_uthmani_simple": "من نطفة خلقه فقدره"
        },
        {
            "id": 5778,
            "verse_key": "80:20",
            "text_uthmani_simple": "ثم السبيل يسره"
        },
        {
            "id": 5779,
            "verse_key": "80:21",
            "text_uthmani_simple": "ثم اماته فاقبره"
        },
        {
            "id": 5780,
            "verse_key": "80:22",
            "text_uthmani_simple": "ثم اذا شاء انشره"
        },
        {
            "id": 5781,
            "verse_key": "80:23",
            "text_uthmani_simple": "كلا لما يقض ما امره"
        },
        {
            "id": 5782,
            "verse_key": "80:24",
            "text_uthmani_simple": "فلينظر الانسن الى طعامه"
        },
        {
            "id": 5783,
            "verse_key": "80:25",
            "text_uthmani_simple": "انا صببنا الماء صبا"
        },
        {
            "id": 5784,
            "verse_key": "80:26",
            "text_uthmani_simple": "ثم شققنا الارض شقا"
        },
        {
            "id": 5785,
            "verse_key": "80:27",
            "text_uthmani_simple": "فانۢبتنا فيها حبا"
        },
        {
            "id": 5786,
            "verse_key": "80:28",
            "text_uthmani_simple": "وعنبا وقضبا"
        },
        {
            "id": 5787,
            "verse_key": "80:29",
            "text_uthmani_simple": "وزيتونا ونخلا"
        },
        {
            "id": 5788,
            "verse_key": "80:30",
            "text_uthmani_simple": "وحدائق غلبا"
        },
        {
            "id": 5789,
            "verse_key": "80:31",
            "text_uthmani_simple": "وفكهة وابا"
        },
        {
            "id": 5790,
            "verse_key": "80:32",
            "text_uthmani_simple": "متعا لكم ولانعمكم"
        },
        {
            "id": 5791,
            "verse_key": "80:33",
            "text_uthmani_simple": "فاذا جاءت الصاخة"
        },
        {
            "id": 5792,
            "verse_key": "80:34",
            "text_uthmani_simple": "يوم يفر المرء من اخيه"
        },
        {
            "id": 5793,
            "verse_key": "80:35",
            "text_uthmani_simple": "وامه وابيه"
        },
        {
            "id": 5794,
            "verse_key": "80:36",
            "text_uthmani_simple": "وصحبته وبنيه"
        },
        {
            "id": 5795,
            "verse_key": "80:37",
            "text_uthmani_simple": "لكل امرئ منهم يومئذ شان يغنيه"
        },
        {
            "id": 5796,
            "verse_key": "80:38",
            "text_uthmani_simple": "وجوه يومئذ مسفرة"
        },
        {
            "id": 5797,
            "verse_key": "80:39",
            "text_uthmani_simple": "ضاحكة مستبشرة"
        },
        {
            "id": 5798,
            "verse_key": "80:40",
            "text_uthmani_simple": "ووجوه يومئذ عليها غبرة"
        },
        {
            "id": 5799,
            "verse_key": "80:41",
            "text_uthmani_simple": "ترهقها قترة"
        },
        {
            "id": 5800,
            "verse_key": "80:42",
            "text_uthmani_simple": "او۟لئك هم الكفرة الفجرة"
        }
    ],
