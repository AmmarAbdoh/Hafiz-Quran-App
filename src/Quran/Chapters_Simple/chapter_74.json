{
    "verses": [
        {
            "id": 5496,
            "verse_key": "74:1",
            "text_uthmani_simple": "يايها المدثر"
        },
        {
            "id": 5497,
            "verse_key": "74:2",
            "text_uthmani_simple": "قم فانذر"
        },
        {
            "id": 5498,
            "verse_key": "74:3",
            "text_uthmani_simple": "وربك فكبر"
        },
        {
            "id": 5499,
            "verse_key": "74:4",
            "text_uthmani_simple": "وثيابك فطهر"
        },
        {
            "id": 5500,
            "verse_key": "74:5",
            "text_uthmani_simple": "والرجز فاهجر"
        },
        {
            "id": 5501,
            "verse_key": "74:6",
            "text_uthmani_simple": "ولا تمنن تستكثر"
        },
        {
            "id": 5502,
            "verse_key": "74:7",
            "text_uthmani_simple": "ولربك فاصبر"
        },
        {
            "id": 5503,
            "verse_key": "74:8",
            "text_uthmani_simple": "فاذا نقر فى الناقور"
        },
        {
            "id": 5504,
            "verse_key": "74:9",
            "text_uthmani_simple": "فذلك يومئذ يوم عسير"
        },
        {
            "id": 5505,
            "verse_key": "74:10",
            "text_uthmani_simple": "على الكفرين غير يسير"
        },
        {
            "id": 5506,
            "verse_key": "74:11",
            "text_uthmani_simple": "ذرنى ومن خلقت وحيدا"
        },
        {
            "id": 5507,
            "verse_key": "74:12",
            "text_uthmani_simple": "وجعلت له مالا ممدودا"
        },
        {
            "id": 5508,
            "verse_key": "74:13",
            "text_uthmani_simple": "وبنين شهودا"
        },
        {
            "id": 5509,
            "verse_key": "74:14",
            "text_uthmani_simple": "ومهدت له تمهيدا"
        },
        {
            "id": 5510,
            "verse_key": "74:15",
            "text_uthmani_simple": "ثم يطمع ان ازيد"
        },
        {
            "id": 5511,
            "verse_key": "74:16",
            "text_uthmani_simple": "كلا انه كان لءايتنا عنيدا"
        },
        {
            "id": 5512,
            "verse_key": "74:17",
            "text_uthmani_simple": "سارهقه صعودا"
        },
        {
            "id": 5513,
            "verse_key": "74:18",
            "text_uthmani_simple": "انه فكر وقدر"
        },
        {
            "id": 5514,
            "verse_key": "74:19",
            "text_uthmani_simple": "فقتل كيف قدر"
        },
        {
            "id": 5515,
            "verse_key": "74:20",
            "text_uthmani_simple": "ثم قتل كيف قدر"
        },
        {
            "id": 5516,
            "verse_key": "74:21",
            "text_uthmani_simple": "ثم نظر"
        },
        {
            "id": 5517,
            "verse_key": "74:22",
            "text_uthmani_simple": "ثم عبس وبسر"
        },
        {
            "id": 5518,
            "verse_key": "74:23",
            "text_uthmani_simple": "ثم ادبر واستكبر"
        },
        {
            "id": 5519,
            "verse_key": "74:24",
            "text_uthmani_simple": "فقال ان هذا الا سحر يؤثر"
        },
        {
            "id": 5520,
            "verse_key": "74:25",
            "text_uthmani_simple": "ان هذا الا قول البشر"
        },
        {
            "id": 5521,
            "verse_key": "74:26",
            "text_uthmani_simple": "ساصليه سقر"
        },
        {
            "id": 5522,
            "verse_key": "74:27",
            "text_uthmani_simple": "وما ادرىك ما سقر"
        },
        {
            "id": 5523,
            "verse_key": "74:28",
            "text_uthmani_simple": "لا تبقى ولا تذر"
        },
        {
            "id": 5524,
            "verse_key": "74:29",
            "text_uthmani_simple": "لواحة للبشر"
        },
        {
            "id": 5525,
            "verse_key": "74:30",
            "text_uthmani_simple": "عليها تسعة عشر"
        },
        {
            "id": 5526,
            "verse_key": "74:31",
            "text_uthmani_simple": "وما جعلنا اصحب النار الا ملئكة وما جعلنا عدتهم الا فتنة للذين كفروا۟ ليستيقن الذين اوتوا۟ الكتب ويزداد الذين ءامنوا۟ ايمنا ولا يرتاب الذين اوتوا۟ الكتب والمؤمنون وليقول الذين فى قلوبهم مرض والكفرون ماذا اراد الله بهذا مثلا كذلك يضل الله من يشاء ويهدى من يشاء وما يعلم جنود ربك الا هو وما هى الا ذكرى للبشر"
        },
        {
            "id": 5527,
            "verse_key": "74:32",
            "text_uthmani_simple": "كلا والقمر"
        },
        {
            "id": 5528,
            "verse_key": "74:33",
            "text_uthmani_simple": "واليل اذ ادبر"
        },
        {
            "id": 5529,
            "verse_key": "74:34",
            "text_uthmani_simple": "والصبح اذا اسفر"
        },
        {
            "id": 5530,
            "verse_key": "74:35",
            "text_uthmani_simple": "انها لاحدى الكبر"
        },
        {
            "id": 5531,
            "verse_key": "74:36",
            "text_uthmani_simple": "نذيرا للبشر"
        },
        {
            "id": 5532,
            "verse_key": "74:37",
            "text_uthmani_simple": "لمن شاء منكم ان يتقدم او يتاخر"
        },
        {
            "id": 5533,
            "verse_key": "74:38",
            "text_uthmani_simple": "كل نفسۭ بما كسبت رهينة"
        },
        {
            "id": 5534,
            "verse_key": "74:39",
            "text_uthmani_simple": "الا اصحب اليمين"
        },
        {
            "id": 5535,
            "verse_key": "74:40",
            "text_uthmani_simple": "فى جنت يتساءلون"
        },
        {
            "id": 5536,
            "verse_key": "74:41",
            "text_uthmani_simple": "عن المجرمين"
        },
        {
            "id": 5537,
            "verse_key": "74:42",
            "text_uthmani_simple": "ما سلككم فى سقر"
        },
        {
            "id": 5538,
            "verse_key": "74:43",
            "text_uthmani_simple": "قالوا۟ لم نك من المصلين"
        },
        {
            "id": 5539,
            "verse_key": "74:44",
            "text_uthmani_simple": "ولم نك نطعم المسكين"
        },
        {
            "id": 5540,
            "verse_key": "74:45",
            "text_uthmani_simple": "وكنا نخوض مع الخائضين"
        },
        {
            "id": 5541,
            "verse_key": "74:46",
            "text_uthmani_simple": "وكنا نكذب بيوم الدين"
        },
        {
            "id": 5542,
            "verse_key": "74:47",
            "text_uthmani_simple": "حتى اتىنا اليقين"
        },
        {
            "id": 5543,
            "verse_key": "74:48",
            "text_uthmani_simple": "فما تنفعهم شفعة الشفعين"
        },
        {
            "id": 5544,
            "verse_key": "74:49",
            "text_uthmani_simple": "فما لهم عن التذكرة معرضين"
        },
        {
            "id": 5545,
            "verse_key": "74:50",
            "text_uthmani_simple": "كانهم حمر مستنفرة"
        },
        {
            "id": 5546,
            "verse_key": "74:51",
            "text_uthmani_simple": "فرت من قسورةۭ"
        },
        {
            "id": 5547,
            "verse_key": "74:52",
            "text_uthmani_simple": "بل يريد كل امرئ منهم ان يؤتى صحفا منشرة"
        },
        {
            "id": 5548,
            "verse_key": "74:53",
            "text_uthmani_simple": "كلا بل لا يخافون الءاخرة"
        },
        {
            "id": 5549,
            "verse_key": "74:54",
            "text_uthmani_simple": "كلا انه تذكرة"
        },
        {
            "id": 5550,
            "verse_key": "74:55",
            "text_uthmani_simple": "فمن شاء ذكره"
        },
        {
            "id": 5551,
            "verse_key": "74:56",
            "text_uthmani_simple": "وما يذكرون الا ان يشاء الله هو اهل التقوى واهل المغفرة"
        }
    ],
