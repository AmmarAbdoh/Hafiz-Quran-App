{
    "verses": [
        {
            "id": 5673,
            "verse_key": "78:1",
            "text_uthmani_simple": "عم يتساءلون"
        },
        {
            "id": 5674,
            "verse_key": "78:2",
            "text_uthmani_simple": "عن النبا العظيم"
        },
        {
            "id": 5675,
            "verse_key": "78:3",
            "text_uthmani_simple": "الذى هم فيه مختلفون"
        },
        {
            "id": 5676,
            "verse_key": "78:4",
            "text_uthmani_simple": "كلا سيعلمون"
        },
        {
            "id": 5677,
            "verse_key": "78:5",
            "text_uthmani_simple": "ثم كلا سيعلمون"
        },
        {
            "id": 5678,
            "verse_key": "78:6",
            "text_uthmani_simple": "الم نجعل الارض مهدا"
        },
        {
            "id": 5679,
            "verse_key": "78:7",
            "text_uthmani_simple": "والجبال اوتادا"
        },
        {
            "id": 5680,
            "verse_key": "78:8",
            "text_uthmani_simple": "وخلقنكم ازوجا"
        },
        {
            "id": 5681,
            "verse_key": "78:9",
            "text_uthmani_simple": "وجعلنا نومكم سباتا"
        },
        {
            "id": 5682,
            "verse_key": "78:10",
            "text_uthmani_simple": "وجعلنا اليل لباسا"
        },
        {
            "id": 5683,
            "verse_key": "78:11",
            "text_uthmani_simple": "وجعلنا النهار معاشا"
        },
        {
            "id": 5684,
            "verse_key": "78:12",
            "text_uthmani_simple": "وبنينا فوقكم سبعا شدادا"
        },
        {
            "id": 5685,
            "verse_key": "78:13",
            "text_uthmani_simple": "وجعلنا سراجا وهاجا"
        },
        {
            "id": 5686,
            "verse_key": "78:14",
            "text_uthmani_simple": "وانزلنا من المعصرت ماء ثجاجا"
        },
        {
            "id": 5687,
            "verse_key": "78:15",
            "text_uthmani_simple": "لنخرج به حبا ونباتا"
        },
        {
            "id": 5688,
            "verse_key": "78:16",
            "text_uthmani_simple": "وجنت الفافا"
        },
        {
            "id": 5689,
            "verse_key": "78:17",
            "text_uthmani_simple": "ان يوم الفصل كان ميقتا"
        },
        {
            "id": 5690,
            "verse_key": "78:18",
            "text_uthmani_simple": "يوم ينفخ فى الصور فتاتون افواجا"
        },
        {
            "id": 5691,
            "verse_key": "78:19",
            "text_uthmani_simple": "وفتحت السماء فكانت ابوبا"
        },
        {
            "id": 5692,
            "verse_key": "78:20",
            "text_uthmani_simple": "وسيرت الجبال فكانت سرابا"
        },
        {
            "id": 5693,
            "verse_key": "78:21",
            "text_uthmani_simple": "ان جهنم كانت مرصادا"
        },
        {
            "id": 5694,
            "verse_key": "78:22",
            "text_uthmani_simple": "للطغين مـابا"
        },
        {
            "id": 5695,
            "verse_key": "78:23",
            "text_uthmani_simple": "لبثين فيها احقابا"
        },
        {
            "id": 5696,
            "verse_key": "78:24",
            "text_uthmani_simple": "لا يذوقون فيها بردا ولا شرابا"
        },
        {
            "id": 5697,
            "verse_key": "78:25",
            "text_uthmani_simple": "الا حميما وغساقا"
        },
        {
            "id": 5698,
            "verse_key": "78:26",
            "text_uthmani_simple": "جزاء وفاقا"
        },
        {
            "id": 5699,
            "verse_key": "78:27",
            "text_uthmani_simple": "انهم كانوا۟ لا يرجون حسابا"
        },
        {
            "id": 5700,
            "verse_key": "78:28",
            "text_uthmani_simple": "وكذبوا۟ بـايتنا كذابا"
        },
        {
            "id": 5701,
            "verse_key": "78:29",
            "text_uthmani_simple": "وكل شىء احصينه كتبا"
        },
        {
            "id": 5702,
            "verse_key": "78:30",
            "text_uthmani_simple": "فذوقوا۟ فلن نزيدكم الا عذابا"
        },
        {
            "id": 5703,
            "verse_key": "78:31",
            "text_uthmani_simple": "ان للمتقين مفازا"
        },
        {
            "id": 5704,
            "verse_key": "78:32",
            "text_uthmani_simple": "حدائق واعنبا"
        },
        {
            "id": 5705,
            "verse_key": "78:33",
            "text_uthmani_simple": "وكواعب اترابا"
        },
        {
            "id": 5706,
            "verse_key": "78:34",
            "text_uthmani_simple": "وكاسا دهاقا"
        },
        {
            "id": 5707,
            "verse_key": "78:35",
            "text_uthmani_simple": "لا يسمعون فيها لغوا ولا كذبا"
        },
        {
            "id": 5708,
            "verse_key": "78:36",
            "text_uthmani_simple": "جزاء من ربك عطاء حسابا"
        },
        {
            "id": 5709,
            "verse_key": "78:37",
            "text_uthmani_simple": "رب السموت والارض وما بينهما الرحمن لا يملكون منه خطابا"
        },
        {
            "id": 5710,
            "verse_key": "78:38",
            "text_uthmani_simple": "يوم يقوم الروح والملئكة صفا لا يتكلمون الا من اذن له الرحمن وقال صوابا"
        },
        {
            "id": 5711,
            "verse_key": "78:39",
            "text_uthmani_simple": "ذلك اليوم الحق فمن شاء اتخذ الى ربه مـابا"
        },
        {
            "id": 5712,
            "verse_key": "78:40",
            "text_uthmani_simple": "انا انذرنكم عذابا قريبا يوم ينظر المرء ما قدمت يداه ويقول الكافر يليتنى كنت تربۢا"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "78"
        }
    }
}