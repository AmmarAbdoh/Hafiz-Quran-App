{
    "verses": [
        {
            "id": 6080,
            "verse_key": "93:1",
            "text_uthmani_simple": "والضحى"
        },
        {
            "id": 6081,
            "verse_key": "93:2",
            "text_uthmani_simple": "واليل اذا سجى"
        },
        {
            "id": 6082,
            "verse_key": "93:3",
            "text_uthmani_simple": "ما ودعك ربك وما قلى"
        },
        {
            "id": 6083,
            "verse_key": "93:4",
            "text_uthmani_simple": "وللءاخرة خير لك من الاولى"
        },
        {
            "id": 6084,
            "verse_key": "93:5",
            "text_uthmani_simple": "ولسوف يعطيك ربك فترضى"
        },
        {
            "id": 6085,
            "verse_key": "93:6",
            "text_uthmani_simple": "الم يجدك يتيما فـاوى"
        },
        {
            "id": 6086,
            "verse_key": "93:7",
            "text_uthmani_simple": "ووجدك ضالا فهدى"
        },
        {
            "id": 6087,
            "verse_key": "93:8",
            "text_uthmani_simple": "ووجدك عائلا فاغنى"
        },
        {
            "id": 6088,
            "verse_key": "93:9",
            "text_uthmani_simple": "فاما اليتيم فلا تقهر"
        },
        {
            "id": 6089,
            "verse_key": "93:10",
            "text_uthmani_simple": "واما السائل فلا تنهر"
        },
        {
            "id": 6090,
            "verse_key": "93:11",
            "text_uthmani_simple": "واما بنعمة ربك فحدث"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "93"
        }
    }
}