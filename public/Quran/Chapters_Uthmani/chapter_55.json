{
    "verses": [
        {
            "id": 4902,
            "verse_key": "55:1",
            "text_uthmani": " ٱلرَّحْمَـٰنُ"
        },
        {
            "id": 4903,
            "verse_key": "55:2",
            "text_uthmani": "عَلَّمَ ٱلْقُرْءَانَ"
        },
        {
            "id": 4904,
            "verse_key": "55:3",
            "text_uthmani": "خَلَقَ ٱلْإِنسَـٰنَ"
        },
        {
            "id": 4905,
            "verse_key": "55:4",
            "text_uthmani": "عَلَّمَهُ ٱلْبَيَانَ"
        },
        {
            "id": 4906,
            "verse_key": "55:5",
            "text_uthmani": "ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍ"
        },
        {
            "id": 4907,
            "verse_key": "55:6",
            "text_uthmani": "وَٱلنَّجْمُ وَٱلشَّجَرُ يَسْجُدَانِ"
        },
        {
            "id": 4908,
            "verse_key": "55:7",
            "text_uthmani": "وَٱلسَّمَآءَ رَفَعَهَا وَوَضَعَ ٱلْمِيزَانَ"
        },
        {
            "id": 4909,
            "verse_key": "55:8",
            "text_uthmani": "أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ"
        },
        {
            "id": 4910,
            "verse_key": "55:9",
            "text_uthmani": "وَأَقِيمُوا۟ ٱلْوَزْنَ بِٱلْقِسْطِ وَلَا تُخْسِرُوا۟ ٱلْمِيزَانَ"
        },
        {
            "id": 4911,
            "verse_key": "55:10",
            "text_uthmani": "وَٱلْأَرْضَ وَضَعَهَا لِلْأَنَامِ"
        },
        {
            "id": 4912,
            "verse_key": "55:11",
            "text_uthmani": "فِيهَا فَـٰكِهَةٌ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ"
        },
        {
            "id": 4913,
            "verse_key": "55:12",
            "text_uthmani": "وَٱلْحَبُّ ذُو ٱلْعَصْفِ وَٱلرَّيْحَانُ"
        },
        {
            "id": 4914,
            "verse_key": "55:13",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4915,
            "verse_key": "55:14",
            "text_uthmani": "خَلَقَ ٱلْإِنسَـٰنَ مِن صَلْصَـٰلٍ كَٱلْفَخَّارِ"
        },
        {
            "id": 4916,
            "verse_key": "55:15",
            "text_uthmani": "وَخَلَقَ ٱلْجَآنَّ مِن مَّارِجٍ مِّن نَّارٍ"
        },
        {
            "id": 4917,
            "verse_key": "55:16",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4918,
            "verse_key": "55:17",
            "text_uthmani": "رَبُّ ٱلْمَشْرِقَيْنِ وَرَبُّ ٱلْمَغْرِبَيْنِ"
        },
        {
            "id": 4919,
            "verse_key": "55:18",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4920,
            "verse_key": "55:19",
            "text_uthmani": "مَرَجَ ٱلْبَحْرَيْنِ يَلْتَقِيَانِ"
        },
        {
            "id": 4921,
            "verse_key": "55:20",
            "text_uthmani": "بَيْنَهُمَا بَرْزَخٌ لَّا يَبْغِيَانِ"
        },
        {
            "id": 4922,
            "verse_key": "55:21",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4923,
            "verse_key": "55:22",
            "text_uthmani": "يَخْرُجُ مِنْهُمَا ٱللُّؤْلُؤُ وَٱلْمَرْجَانُ"
        },
        {
            "id": 4924,
            "verse_key": "55:23",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4925,
            "verse_key": "55:24",
            "text_uthmani": "وَلَهُ ٱلْجَوَارِ ٱلْمُنشَـَٔاتُ فِى ٱلْبَحْرِ كَٱلْأَعْلَـٰمِ"
        },
        {
            "id": 4926,
            "verse_key": "55:25",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4927,
            "verse_key": "55:26",
            "text_uthmani": "كُلُّ مَنْ عَلَيْهَا فَانٍ"
        },
        {
            "id": 4928,
            "verse_key": "55:27",
            "text_uthmani": "وَيَبْقَىٰ وَجْهُ رَبِّكَ ذُو ٱلْجَلَـٰلِ وَٱلْإِكْرَامِ"
        },
        {
            "id": 4929,
            "verse_key": "55:28",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4930,
            "verse_key": "55:29",
            "text_uthmani": "يَسْـَٔلُهُۥ مَن فِى ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ كُلَّ يَوْمٍ هُوَ فِى شَأْنٍ"
        },
        {
            "id": 4931,
            "verse_key": "55:30",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4932,
            "verse_key": "55:31",
            "text_uthmani": "سَنَفْرُغُ لَكُمْ أَيُّهَ ٱلثَّقَلَانِ"
        },
        {
            "id": 4933,
            "verse_key": "55:32",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4934,
            "verse_key": "55:33",
            "text_uthmani": "يَـٰمَعْشَرَ ٱلْجِنِّ وَٱلْإِنسِ إِنِ ٱسْتَطَعْتُمْ أَن تَنفُذُوا۟ مِنْ أَقْطَارِ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ فَٱنفُذُوا۟ ۚ لَا تَنفُذُونَ إِلَّا بِسُلْطَـٰنٍ"
        },
        {
            "id": 4935,
            "verse_key": "55:34",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4936,
            "verse_key": "55:35",
            "text_uthmani": "يُرْسَلُ عَلَيْكُمَا شُوَاظٌ مِّن نَّارٍ وَنُحَاسٌ فَلَا تَنتَصِرَانِ"
        },
        {
            "id": 4937,
            "verse_key": "55:36",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4938,
            "verse_key": "55:37",
            "text_uthmani": "فَإِذَا ٱنشَقَّتِ ٱلسَّمَآءُ فَكَانَتْ وَرْدَةً كَٱلدِّهَانِ"
        },
        {
            "id": 4939,
            "verse_key": "55:38",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4940,
            "verse_key": "55:39",
            "text_uthmani": "فَيَوْمَئِذٍ لَّا يُسْـَٔلُ عَن ذَنۢبِهِۦٓ إِنسٌ وَلَا جَآنٌّ"
        },
        {
            "id": 4941,
            "verse_key": "55:40",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4942,
            "verse_key": "55:41",
            "text_uthmani": "يُعْرَفُ ٱلْمُجْرِمُونَ بِسِيمَـٰهُمْ فَيُؤْخَذُ بِٱلنَّوَٰصِى وَٱلْأَقْدَامِ"
        },
        {
            "id": 4943,
            "verse_key": "55:42",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4944,
            "verse_key": "55:43",
            "text_uthmani": "هَـٰذِهِۦ جَهَنَّمُ ٱلَّتِى يُكَذِّبُ بِهَا ٱلْمُجْرِمُونَ"
        },
        {
            "id": 4945,
            "verse_key": "55:44",
            "text_uthmani": "يَطُوفُونَ بَيْنَهَا وَبَيْنَ حَمِيمٍ ءَانٍ"
        },
        {
            "id": 4946,
            "verse_key": "55:45",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4947,
            "verse_key": "55:46",
            "text_uthmani": "وَلِمَنْ خَافَ مَقَامَ رَبِّهِۦ جَنَّتَانِ"
        },
        {
            "id": 4948,
            "verse_key": "55:47",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4949,
            "verse_key": "55:48",
            "text_uthmani": "ذَوَاتَآ أَفْنَانٍ"
        },
        {
            "id": 4950,
            "verse_key": "55:49",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4951,
            "verse_key": "55:50",
            "text_uthmani": "فِيهِمَا عَيْنَانِ تَجْرِيَانِ"
        },
        {
            "id": 4952,
            "verse_key": "55:51",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4953,
            "verse_key": "55:52",
            "text_uthmani": "فِيهِمَا مِن كُلِّ فَـٰكِهَةٍ زَوْجَانِ"
        },
        {
            "id": 4954,
            "verse_key": "55:53",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4955,
            "verse_key": "55:54",
            "text_uthmani": "مُتَّكِـِٔينَ عَلَىٰ فُرُشٍۭ بَطَآئِنُهَا مِنْ إِسْتَبْرَقٍ ۚ وَجَنَى ٱلْجَنَّتَيْنِ دَانٍ"
        },
        {
            "id": 4956,
            "verse_key": "55:55",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4957,
            "verse_key": "55:56",
            "text_uthmani": "فِيهِنَّ قَـٰصِرَٰتُ ٱلطَّرْفِ لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَآنٌّ"
        },
        {
            "id": 4958,
            "verse_key": "55:57",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4959,
            "verse_key": "55:58",
            "text_uthmani": "كَأَنَّهُنَّ ٱلْيَاقُوتُ وَٱلْمَرْجَانُ"
        },
        {
            "id": 4960,
            "verse_key": "55:59",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4961,
            "verse_key": "55:60",
            "text_uthmani": "هَلْ جَزَآءُ ٱلْإِحْسَـٰنِ إِلَّا ٱلْإِحْسَـٰنُ"
        },
        {
            "id": 4962,
            "verse_key": "55:61",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4963,
            "verse_key": "55:62",
            "text_uthmani": "وَمِن دُونِهِمَا جَنَّتَانِ"
        },
        {
            "id": 4964,
            "verse_key": "55:63",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4965,
            "verse_key": "55:64",
            "text_uthmani": "مُدْهَآمَّتَانِ"
        },
        {
            "id": 4966,
            "verse_key": "55:65",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4967,
            "verse_key": "55:66",
            "text_uthmani": "فِيهِمَا عَيْنَانِ نَضَّاخَتَانِ"
        },
        {
            "id": 4968,
            "verse_key": "55:67",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4969,
            "verse_key": "55:68",
            "text_uthmani": "فِيهِمَا فَـٰكِهَةٌ وَنَخْلٌ وَرُمَّانٌ"
        },
        {
            "id": 4970,
            "verse_key": "55:69",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4971,
            "verse_key": "55:70",
            "text_uthmani": "فِيهِنَّ خَيْرَٰتٌ حِسَانٌ"
        },
        {
            "id": 4972,
            "verse_key": "55:71",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4973,
            "verse_key": "55:72",
            "text_uthmani": "حُورٌ مَّقْصُورَٰتٌ فِى ٱلْخِيَامِ"
        },
        {
            "id": 4974,
            "verse_key": "55:73",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4975,
            "verse_key": "55:74",
            "text_uthmani": "لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَآنٌّ"
        },
        {
            "id": 4976,
            "verse_key": "55:75",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4977,
            "verse_key": "55:76",
            "text_uthmani": "مُتَّكِـِٔينَ عَلَىٰ رَفْرَفٍ خُضْرٍ وَعَبْقَرِىٍّ حِسَانٍ"
        },
        {
            "id": 4978,
            "verse_key": "55:77",
            "text_uthmani": "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ"
        },
        {
            "id": 4979,
            "verse_key": "55:78",
            "text_uthmani": "تَبَـٰرَكَ ٱسْمُ رَبِّكَ ذِى ٱلْجَلَـٰلِ وَٱلْإِكْرَامِ"
        }
    ],
