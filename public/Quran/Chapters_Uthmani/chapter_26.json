        {
            "id": 3136,
            "verse_key": "26:204",
            "text_uthmani": "أَفَبِعَذَابِنَا يَسْتَعْجِلُونَ"
        },
        {
            "id": 3137,
            "verse_key": "26:205",
            "text_uthmani": "أَفَرَءَيْتَ إِن مَّتَّعْنَـٰهُمْ سِنِينَ"
        },
        {
            "id": 3138,
            "verse_key": "26:206",
            "text_uthmani": "ثُمَّ جَآءَهُم مَّا كَانُوا۟ يُوعَدُونَ"
        },
        {
            "id": 3139,
            "verse_key": "26:207",
            "text_uthmani": "مَآ أَغْنَىٰ عَنْهُم مَّا كَانُوا۟ يُمَتَّعُونَ"
        },
        {
            "id": 3140,
            "verse_key": "26:208",
            "text_uthmani": "وَمَآ أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا لَهَا مُنذِرُونَ"
        },
        {
            "id": 3141,
            "verse_key": "26:209",
            "text_uthmani": "ذِكْرَىٰ وَمَا كُنَّا ظَـٰلِمِينَ"
        },
        {
            "id": 3142,
            "verse_key": "26:210",
            "text_uthmani": "وَمَا تَنَزَّلَتْ بِهِ ٱلشَّيَـٰطِينُ"
        },
        {
            "id": 3143,
            "verse_key": "26:211",
            "text_uthmani": "وَمَا يَنۢبَغِى لَهُمْ وَمَا يَسْتَطِيعُونَ"
        },
        {
            "id": 3144,
            "verse_key": "26:212",
            "text_uthmani": "إِنَّهُمْ عَنِ ٱلسَّمْعِ لَمَعْزُولُونَ"
        },
        {
            "id": 3145,
            "verse_key": "26:213",
            "text_uthmani": "فَلَا تَدْعُ مَعَ ٱللَّهِ إِلَـٰهًا ءَاخَرَ فَتَكُونَ مِنَ ٱلْمُعَذَّبِينَ"
        },
        {
            "id": 3146,
            "verse_key": "26:214",
            "text_uthmani": "وَأَنذِرْ عَشِيرَتَكَ ٱلْأَقْرَبِينَ"
        },
        {
            "id": 3147,
            "verse_key": "26:215",
            "text_uthmani": "وَٱخْفِضْ جَنَاحَكَ لِمَنِ ٱتَّبَعَكَ مِنَ ٱلْمُؤْمِنِينَ"
        },
        {
            "id": 3148,
            "verse_key": "26:216",
            "text_uthmani": "فَإِنْ عَصَوْكَ فَقُلْ إِنِّى بَرِىٓءٌ مِّمَّا تَعْمَلُونَ"
        },
        {
            "id": 3149,
            "verse_key": "26:217",
            "text_uthmani": "وَتَوَكَّلْ عَلَى ٱلْعَزِيزِ ٱلرَّحِيمِ"
        },
        {
            "id": 3150,
            "verse_key": "26:218",
            "text_uthmani": "ٱلَّذِى يَرَىٰكَ حِينَ تَقُومُ"
        },
        {
            "id": 3151,
            "verse_key": "26:219",
            "text_uthmani": "وَتَقَلُّبَكَ فِى ٱلسَّـٰجِدِينَ"
        },
        {
            "id": 3152,
            "verse_key": "26:220",
            "text_uthmani": "إِنَّهُۥ هُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ"
