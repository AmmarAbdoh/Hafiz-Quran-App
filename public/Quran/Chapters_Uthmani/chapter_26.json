        {
            "id": 3095,
            "verse_key": "26:163",
            "text_uthmani": "فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ"
        },
        {
            "id": 3096,
            "verse_key": "26:164",
            "text_uthmani": "وَمَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ ۖ إِنْ أَجْرِىَ إِلَّا عَلَىٰ رَبِّ ٱلْعَـٰلَمِينَ"
        },
        {
            "id": 3097,
            "verse_key": "26:165",
            "text_uthmani": "أَتَأْتُونَ ٱلذُّكْرَانَ مِنَ ٱلْعَـٰلَمِينَ"
        },
        {
            "id": 3098,
            "verse_key": "26:166",
            "text_uthmani": "وَتَذَرُونَ مَا خَلَقَ لَكُمْ رَبُّكُم مِّنْ أَزْوَٰجِكُم ۚ بَلْ أَنتُمْ قَوْمٌ عَادُونَ"
        },
        {
            "id": 3099,
            "verse_key": "26:167",
            "text_uthmani": "قَالُوا۟ لَئِن لَّمْ تَنتَهِ يَـٰلُوطُ لَتَكُونَنَّ مِنَ ٱلْمُخْرَجِينَ"
        },
        {
            "id": 3100,
            "verse_key": "26:168",
            "text_uthmani": "قَالَ إِنِّى لِعَمَلِكُم مِّنَ ٱلْقَالِينَ"
        },
        {
            "id": 3101,
            "verse_key": "26:169",
            "text_uthmani": "رَبِّ نَجِّنِى وَأَهْلِى مِمَّا يَعْمَلُونَ"
        },
        {
            "id": 3102,
            "verse_key": "26:170",
            "text_uthmani": "فَنَجَّيْنَـٰهُ وَأَهْلَهُۥٓ أَجْمَعِينَ"
        },
        {
            "id": 3103,
            "verse_key": "26:171",
            "text_uthmani": "إِلَّا عَجُوزًا فِى ٱلْغَـٰبِرِينَ"
        },
        {
            "id": 3104,
            "verse_key": "26:172",
            "text_uthmani": "ثُمَّ دَمَّرْنَا ٱلْـَٔاخَرِينَ"
        },
        {
            "id": 3105,
            "verse_key": "26:173",
            "text_uthmani": "وَأَمْطَرْنَا عَلَيْهِم مَّطَرًا ۖ فَسَآءَ مَطَرُ ٱلْمُنذَرِينَ"
        },
        {
            "id": 3106,
            "verse_key": "26:174",
            "text_uthmani": "إِنَّ فِى ذَٰلِكَ لَـَٔايَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ"
        },
        {
            "id": 3107,
