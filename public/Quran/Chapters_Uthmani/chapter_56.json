{
    "verses": [
        {
            "id": 4980,
            "verse_key": "56:1",
            "text_uthmani": " إِذَا وَقَعَتِ ٱلْوَاقِعَةُ"
        },
        {
            "id": 4981,
            "verse_key": "56:2",
            "text_uthmani": "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ"
        },
        {
            "id": 4982,
            "verse_key": "56:3",
            "text_uthmani": "خَافِضَةٌ رَّافِعَةٌ"
        },
        {
            "id": 4983,
            "verse_key": "56:4",
            "text_uthmani": "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّا"
        },
        {
            "id": 4984,
            "verse_key": "56:5",
            "text_uthmani": "وَبُسَّتِ ٱلْجِبَالُ بَسًّا"
        },
        {
            "id": 4985,
            "verse_key": "56:6",
            "text_uthmani": "فَكَانَتْ هَبَآءً مُّنۢبَثًّا"
        },
        {
            "id": 4986,
            "verse_key": "56:7",
            "text_uthmani": "وَكُنتُمْ أَزْوَٰجًا ثَلَـٰثَةً"
        },
        {
            "id": 4987,
            "verse_key": "56:8",
            "text_uthmani": "فَأَصْحَـٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَـٰبُ ٱلْمَيْمَنَةِ"
        },
        {
            "id": 4988,
            "verse_key": "56:9",
            "text_uthmani": "وَأَصْحَـٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَـٰبُ ٱلْمَشْـَٔمَةِ"
        },
        {
            "id": 4989,
            "verse_key": "56:10",
            "text_uthmani": "وَٱلسَّـٰبِقُونَ ٱلسَّـٰبِقُونَ"
        },
        {
            "id": 4990,
            "verse_key": "56:11",
            "text_uthmani": "أُو۟لَـٰٓئِكَ ٱلْمُقَرَّبُونَ"
        },
        {
            "id": 4991,
            "verse_key": "56:12",
            "text_uthmani": "فِى جَنَّـٰتِ ٱلنَّعِيمِ"
        },
        {
            "id": 4992,
            "verse_key": "56:13",
            "text_uthmani": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ"
        },
        {
            "id": 4993,
            "verse_key": "56:14",
            "text_uthmani": "وَقَلِيلٌ مِّنَ ٱلْـَٔاخِرِينَ"
        },
        {
            "id": 4994,
            "verse_key": "56:15",
            "text_uthmani": "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ"
        },
        {
            "id": 4995,
            "verse_key": "56:16",
            "text_uthmani": "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَـٰبِلِينَ"
        },
        {
            "id": 4996,
            "verse_key": "56:17",
            "text_uthmani": "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ"
        },
        {
            "id": 4997,
            "verse_key": "56:18",
            "text_uthmani": "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ"
        },
        {
            "id": 4998,
            "verse_key": "56:19",
            "text_uthmani": "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ"
        },
        {
            "id": 4999,
            "verse_key": "56:20",
            "text_uthmani": "وَفَـٰكِهَةٍ مِّمَّا يَتَخَيَّرُونَ"
        },
        {
            "id": 5000,
            "verse_key": "56:21",
            "text_uthmani": "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ"
        },
        {
            "id": 5001,
            "verse_key": "56:22",
            "text_uthmani": "وَحُورٌ عِينٌ"
        },
        {
            "id": 5002,
            "verse_key": "56:23",
            "text_uthmani": "كَأَمْثَـٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ"
        },
        {
            "id": 5003,
            "verse_key": "56:24",
            "text_uthmani": "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ"
        },
        {
            "id": 5004,
            "verse_key": "56:25",
            "text_uthmani": "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا"
        },
        {
            "id": 5005,
            "verse_key": "56:26",
            "text_uthmani": "إِلَّا قِيلًا سَلَـٰمًا سَلَـٰمًا"
        },
        {
            "id": 5006,
            "verse_key": "56:27",
            "text_uthmani": "وَأَصْحَـٰبُ ٱلْيَمِينِ مَآ أَصْحَـٰبُ ٱلْيَمِينِ"
        },
        {
            "id": 5007,
            "verse_key": "56:28",
            "text_uthmani": "فِى سِدْرٍ مَّخْضُودٍ"
        },
        {
            "id": 5008,
            "verse_key": "56:29",
            "text_uthmani": "وَطَلْحٍ مَّنضُودٍ"
        },
        {
            "id": 5009,
            "verse_key": "56:30",
            "text_uthmani": "وَظِلٍّ مَّمْدُودٍ"
        },
        {
            "id": 5010,
            "verse_key": "56:31",
            "text_uthmani": "وَمَآءٍ مَّسْكُوبٍ"
        },
        {
            "id": 5011,
            "verse_key": "56:32",
            "text_uthmani": "وَفَـٰكِهَةٍ كَثِيرَةٍ"
        },
        {
            "id": 5012,
            "verse_key": "56:33",
            "text_uthmani": "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ"
        },
        {
            "id": 5013,
            "verse_key": "56:34",
            "text_uthmani": "وَفُرُشٍ مَّرْفُوعَةٍ"
        },
        {
            "id": 5014,
            "verse_key": "56:35",
            "text_uthmani": "إِنَّآ أَنشَأْنَـٰهُنَّ إِنشَآءً"
        },
        {
            "id": 5015,
            "verse_key": "56:36",
            "text_uthmani": "فَجَعَلْنَـٰهُنَّ أَبْكَارًا"
        },
        {
            "id": 5016,
            "verse_key": "56:37",
            "text_uthmani": "عُرُبًا أَتْرَابًا"
        },
        {
            "id": 5017,
            "verse_key": "56:38",
            "text_uthmani": "لِّأَصْحَـٰبِ ٱلْيَمِينِ"
        },
        {
            "id": 5018,
            "verse_key": "56:39",
            "text_uthmani": "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ"
        },
        {
            "id": 5019,
            "verse_key": "56:40",
            "text_uthmani": "وَثُلَّةٌ مِّنَ ٱلْـَٔاخِرِينَ"
        },
        {
            "id": 5020,
            "verse_key": "56:41",
            "text_uthmani": "وَأَصْحَـٰبُ ٱلشِّمَالِ مَآ أَصْحَـٰبُ ٱلشِّمَالِ"
        },
        {
            "id": 5021,
            "verse_key": "56:42",
            "text_uthmani": "فِى سَمُومٍ وَحَمِيمٍ"
        },
        {
            "id": 5022,
            "verse_key": "56:43",
            "text_uthmani": "وَظِلٍّ مِّن يَحْمُومٍ"
        },
        {
            "id": 5023,
            "verse_key": "56:44",
            "text_uthmani": "لَّا بَارِدٍ وَلَا كَرِيمٍ"
        },
        {
            "id": 5024,
            "verse_key": "56:45",
            "text_uthmani": "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ"
        },
        {
            "id": 5025,
            "verse_key": "56:46",
            "text_uthmani": "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ"
        },
        {
            "id": 5026,
            "verse_key": "56:47",
            "text_uthmani": "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ"
        },
        {
            "id": 5027,
            "verse_key": "56:48",
            "text_uthmani": "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ"
        },
        {
            "id": 5028,
            "verse_key": "56:49",
            "text_uthmani": "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْـَٔاخِرِينَ"
        },
        {
            "id": 5029,
            "verse_key": "56:50",
            "text_uthmani": "لَمَجْمُوعُونَ إِلَىٰ مِيقَـٰتِ يَوْمٍ مَّعْلُومٍ"
        },
        {
            "id": 5030,
            "verse_key": "56:51",
            "text_uthmani": "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ"
        },
        {
            "id": 5031,
            "verse_key": "56:52",
            "text_uthmani": "لَـَٔاكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ"
        },
        {
            "id": 5032,
            "verse_key": "56:53",
            "text_uthmani": "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ"
        },
        {
            "id": 5033,
            "verse_key": "56:54",
            "text_uthmani": "فَشَـٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ"
        },
        {
            "id": 5034,
            "verse_key": "56:55",
            "text_uthmani": "فَشَـٰرِبُونَ شُرْبَ ٱلْهِيمِ"
        },
        {
            "id": 5035,
            "verse_key": "56:56",
            "text_uthmani": "هَـٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ"
        },
        {
            "id": 5036,
            "verse_key": "56:57",
            "text_uthmani": "نَحْنُ خَلَقْنَـٰكُمْ فَلَوْلَا تُصَدِّقُونَ"
        },
        {
            "id": 5037,
            "verse_key": "56:58",
            "text_uthmani": "أَفَرَءَيْتُم مَّا تُمْنُونَ"
        },
        {
            "id": 5038,
            "verse_key": "56:59",
            "text_uthmani": "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَـٰلِقُونَ"
        },
        {
            "id": 5039,
            "verse_key": "56:60",
            "text_uthmani": "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ"
        },
        {
            "id": 5040,
            "verse_key": "56:61",
            "text_uthmani": "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَـٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ"
        },
        {
            "id": 5041,
            "verse_key": "56:62",
            "text_uthmani": "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ"
        },
        {
            "id": 5042,
            "verse_key": "56:63",
            "text_uthmani": "أَفَرَءَيْتُم مَّا تَحْرُثُونَ"
        },
        {
            "id": 5043,
            "verse_key": "56:64",
            "text_uthmani": "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ"
        },
        {
            "id": 5044,
            "verse_key": "56:65",
            "text_uthmani": "لَوْ نَشَآءُ لَجَعَلْنَـٰهُ حُطَـٰمًا فَظَلْتُمْ تَفَكَّهُونَ"
        },
        {
            "id": 5045,
            "verse_key": "56:66",
            "text_uthmani": "إِنَّا لَمُغْرَمُونَ"
        },
        {
            "id": 5046,
            "verse_key": "56:67",
            "text_uthmani": "بَلْ نَحْنُ مَحْرُومُونَ"
        },
        {
            "id": 5047,
            "verse_key": "56:68",
            "text_uthmani": "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ"
        },
        {
            "id": 5048,
            "verse_key": "56:69",
            "text_uthmani": "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ"
        },
        {
            "id": 5049,
            "verse_key": "56:70",
            "text_uthmani": "لَوْ نَشَآءُ جَعَلْنَـٰهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ"
        },
        {
            "id": 5050,
            "verse_key": "56:71",
            "text_uthmani": "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ"
        },
        {
            "id": 5051,
            "verse_key": "56:72",
            "text_uthmani": "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ"
        },
        {
            "id": 5052,
            "verse_key": "56:73",
            "text_uthmani": "نَحْنُ جَعَلْنَـٰهَا تَذْكِرَةً وَمَتَـٰعًا لِّلْمُقْوِينَ"
        },
        {
            "id": 5053,
            "verse_key": "56:74",
            "text_uthmani": "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ"
        },
        {
            "id": 5054,
            "verse_key": "56:75",
            "text_uthmani": "۞ فَلَآ أُقْسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ"
        },
        {
            "id": 5055,
            "verse_key": "56:76",
            "text_uthmani": "وَإِنَّهُۥ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ"
        },
        {
            "id": 5056,
            "verse_key": "56:77",
            "text_uthmani": "إِنَّهُۥ لَقُرْءَانٌ كَرِيمٌ"
        },
        {
            "id": 5057,
            "verse_key": "56:78",
            "text_uthmani": "فِى كِتَـٰبٍ مَّكْنُونٍ"
        },
        {
            "id": 5058,
            "verse_key": "56:79",
            "text_uthmani": "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ"
        },
        {
            "id": 5059,
            "verse_key": "56:80",
            "text_uthmani": "تَنزِيلٌ مِّن رَّبِّ ٱلْعَـٰلَمِينَ"
        },
        {
            "id": 5060,
            "verse_key": "56:81",
            "text_uthmani": "أَفَبِهَـٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ"
        },
        {
            "id": 5061,
            "verse_key": "56:82",
            "text_uthmani": "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ"
        },
        {
            "id": 5062,
            "verse_key": "56:83",
            "text_uthmani": "فَلَوْلَآ إِذَا بَلَغَتِ ٱلْحُلْقُومَ"
        },
        {
            "id": 5063,
            "verse_key": "56:84",
            "text_uthmani": "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ"
        },
        {
            "id": 5064,
            "verse_key": "56:85",
            "text_uthmani": "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَـٰكِن لَّا تُبْصِرُونَ"
        },
        {
            "id": 5065,
            "verse_key": "56:86",
            "text_uthmani": "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ"
        },
        {
            "id": 5066,
            "verse_key": "56:87",
            "text_uthmani": "تَرْجِعُونَهَآ إِن كُنتُمْ صَـٰدِقِينَ"
        },
        {
            "id": 5067,
            "verse_key": "56:88",
            "text_uthmani": "فَأَمَّآ إِن كَانَ مِنَ ٱلْمُقَرَّبِينَ"
        },
        {
            "id": 5068,
            "verse_key": "56:89",
            "text_uthmani": "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ"
        },
        {
            "id": 5069,
            "verse_key": "56:90",
            "text_uthmani": "وَأَمَّآ إِن كَانَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ"
        },
        {
            "id": 5070,
            "verse_key": "56:91",
            "text_uthmani": "فَسَلَـٰمٌ لَّكَ مِنْ أَصْحَـٰبِ ٱلْيَمِينِ"
        },
        {
            "id": 5071,
            "verse_key": "56:92",
            "text_uthmani": "وَأَمَّآ إِن كَانَ مِنَ ٱلْمُكَذِّبِينَ ٱلضَّآلِّينَ"
        },
        {
            "id": 5072,
            "verse_key": "56:93",
            "text_uthmani": "فَنُزُلٌ مِّنْ حَمِيمٍ"
        },
        {
            "id": 5073,
            "verse_key": "56:94",
            "text_uthmani": "وَتَصْلِيَةُ جَحِيمٍ"
        },
        {
