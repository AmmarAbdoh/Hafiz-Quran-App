{
    "verses": [
        {
            "id": 5885,
            "verse_key": "84:1",
            "text_uthmani_simple": "اذا السماء انشقت"
        },
        {
            "id": 5886,
            "verse_key": "84:2",
            "text_uthmani_simple": "واذنت لربها وحقت"
        },
        {
            "id": 5887,
            "verse_key": "84:3",
            "text_uthmani_simple": "واذا الارض مدت"
        },
        {
            "id": 5888,
            "verse_key": "84:4",
            "text_uthmani_simple": "والقت ما فيها وتخلت"
        },
        {
            "id": 5889,
            "verse_key": "84:5",
            "text_uthmani_simple": "واذنت لربها وحقت"
        },
        {
            "id": 5890,
            "verse_key": "84:6",
            "text_uthmani_simple": "يايها الانسن انك كادح الى ربك كدحا فملقيه"
        },
        {
            "id": 5891,
            "verse_key": "84:7",
            "text_uthmani_simple": "فاما من اوتى كتبه بيمينه"
        },
        {
            "id": 5892,
            "verse_key": "84:8",
            "text_uthmani_simple": "فسوف يحاسب حسابا يسيرا"
        },
        {
            "id": 5893,
            "verse_key": "84:9",
            "text_uthmani_simple": "وينقلب الى اهله مسرورا"
        },
        {
            "id": 5894,
            "verse_key": "84:10",
            "text_uthmani_simple": "واما من اوتى كتبه وراء ظهره"
        },
        {
            "id": 5895,
            "verse_key": "84:11",
            "text_uthmani_simple": "فسوف يدعوا۟ ثبورا"
        },
        {
            "id": 5896,
            "verse_key": "84:12",
            "text_uthmani_simple": "ويصلى سعيرا"
        },
        {
            "id": 5897,
            "verse_key": "84:13",
            "text_uthmani_simple": "انه كان فى اهله مسرورا"
        },
        {
            "id": 5898,
            "verse_key": "84:14",
            "text_uthmani_simple": "انه ظن ان لن يحور"
        },
        {
            "id": 5899,
            "verse_key": "84:15",
            "text_uthmani_simple": "بلى ان ربه كان به بصيرا"
        },
        {
            "id": 5900,
            "verse_key": "84:16",
            "text_uthmani_simple": "فلا اقسم بالشفق"
        },
        {
            "id": 5901,
            "verse_key": "84:17",
            "text_uthmani_simple": "واليل وما وسق"
        },
        {
            "id": 5902,
            "verse_key": "84:18",
            "text_uthmani_simple": "والقمر اذا اتسق"
        },
        {
            "id": 5903,
            "verse_key": "84:19",
            "text_uthmani_simple": "لتركبن طبقا عن طبق"
        },
        {
            "id": 5904,
            "verse_key": "84:20",
            "text_uthmani_simple": "فما لهم لا يؤمنون"
        },
        {
            "id": 5905,
            "verse_key": "84:21",
            "text_uthmani_simple": "واذا قرئ عليهم القرءان لا يسجدون"
        },
        {
            "id": 5906,
            "verse_key": "84:22",
            "text_uthmani_simple": "بل الذين كفروا۟ يكذبون"
        },
        {
            "id": 5907,
            "verse_key": "84:23",
            "text_uthmani_simple": "والله اعلم بما يوعون"
        },
        {
            "id": 5908,
            "verse_key": "84:24",
            "text_uthmani_simple": "فبشرهم بعذاب اليم"
        },
        {
            "id": 5909,
            "verse_key": "84:25",
            "text_uthmani_simple": "الا الذين ءامنوا۟ وعملوا۟ الصلحت لهم اجر غير ممنونۭ"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "84"
        }
    }
}