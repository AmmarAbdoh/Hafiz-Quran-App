{
    "verses": [
        {
            "id": 5324,
            "verse_key": "69:1",
            "text_uthmani_simple": "الحاقة"
        },
        {
            "id": 5325,
            "verse_key": "69:2",
            "text_uthmani_simple": "ما الحاقة"
        },
        {
            "id": 5326,
            "verse_key": "69:3",
            "text_uthmani_simple": "وما ادرىك ما الحاقة"
        },
        {
            "id": 5327,
            "verse_key": "69:4",
            "text_uthmani_simple": "كذبت ثمود وعادۢ بالقارعة"
        },
        {
            "id": 5328,
            "verse_key": "69:5",
            "text_uthmani_simple": "فاما ثمود فاهلكوا۟ بالطاغية"
        },
        {
            "id": 5329,
            "verse_key": "69:6",
            "text_uthmani_simple": "واما عاد فاهلكوا۟ بريح صرصر عاتية"
        },
        {
            "id": 5330,
            "verse_key": "69:7",
            "text_uthmani_simple": "سخرها عليهم سبع ليال وثمنية ايام حسوما فترى القوم فيها صرعى كانهم اعجاز نخل خاوية"
        },
        {
            "id": 5331,
            "verse_key": "69:8",
            "text_uthmani_simple": "فهل ترى لهم منۢ باقية"
        },
        {
            "id": 5332,
            "verse_key": "69:9",
            "text_uthmani_simple": "وجاء فرعون ومن قبله والمؤتفكت بالخاطئة"
        },
        {
            "id": 5333,
            "verse_key": "69:10",
            "text_uthmani_simple": "فعصوا۟ رسول ربهم فاخذهم اخذة رابية"
        },
        {
            "id": 5334,
            "verse_key": "69:11",
            "text_uthmani_simple": "انا لما طغا الماء حملنكم فى الجارية"
        },
        {
            "id": 5335,
            "verse_key": "69:12",
            "text_uthmani_simple": "لنجعلها لكم تذكرة وتعيها اذن وعية"
        },
        {
            "id": 5336,
            "verse_key": "69:13",
            "text_uthmani_simple": "فاذا نفخ فى الصور نفخة وحدة"
        },
        {
            "id": 5337,
            "verse_key": "69:14",
            "text_uthmani_simple": "وحملت الارض والجبال فدكتا دكة وحدة"
        },
        {
            "id": 5338,
            "verse_key": "69:15",
            "text_uthmani_simple": "فيومئذ وقعت الواقعة"
        },
        {
            "id": 5339,
            "verse_key": "69:16",
            "text_uthmani_simple": "وانشقت السماء فهى يومئذ واهية"
        },
        {
            "id": 5340,
            "verse_key": "69:17",
            "text_uthmani_simple": "والملك على ارجائها ويحمل عرش ربك فوقهم يومئذ ثمنية"
        },
        {
            "id": 5341,
            "verse_key": "69:18",
            "text_uthmani_simple": "يومئذ تعرضون لا تخفى منكم خافية"
        },
        {
            "id": 5342,
            "verse_key": "69:19",
            "text_uthmani_simple": "فاما من اوتى كتبه بيمينه فيقول هاؤم اقرءوا۟ كتبيه"
        },
        {
            "id": 5343,
            "verse_key": "69:20",
            "text_uthmani_simple": "انى ظننت انى ملق حسابيه"
        },
        {
            "id": 5344,
            "verse_key": "69:21",
            "text_uthmani_simple": "فهو فى عيشة راضية"
        },
        {
            "id": 5345,
            "verse_key": "69:22",
            "text_uthmani_simple": "فى جنة عالية"
        },
        {
            "id": 5346,
            "verse_key": "69:23",
            "text_uthmani_simple": "قطوفها دانية"
        },
        {
            "id": 5347,
            "verse_key": "69:24",
            "text_uthmani_simple": "كلوا۟ واشربوا۟ هنيـۢا بما اسلفتم فى الايام الخالية"
        },
        {
            "id": 5348,
            "verse_key": "69:25",
            "text_uthmani_simple": "واما من اوتى كتبه بشماله فيقول يليتنى لم اوت كتبيه"
        },
        {
            "id": 5349,
            "verse_key": "69:26",
            "text_uthmani_simple": "ولم ادر ما حسابيه"
        },
        {
            "id": 5350,
            "verse_key": "69:27",
            "text_uthmani_simple": "يليتها كانت القاضية"
        },
        {
            "id": 5351,
            "verse_key": "69:28",
            "text_uthmani_simple": "ما اغنى عنى ماليه"
        },
        {
            "id": 5352,
            "verse_key": "69:29",
            "text_uthmani_simple": "هلك عنى سلطنيه"
        },
        {
            "id": 5353,
            "verse_key": "69:30",
            "text_uthmani_simple": "خذوه فغلوه"
        },
        {
            "id": 5354,
            "verse_key": "69:31",
            "text_uthmani_simple": "ثم الجحيم صلوه"
        },
        {
            "id": 5355,
            "verse_key": "69:32",
            "text_uthmani_simple": "ثم فى سلسلة ذرعها سبعون ذراعا فاسلكوه"
        },
        {
            "id": 5356,
            "verse_key": "69:33",
            "text_uthmani_simple": "انه كان لا يؤمن بالله العظيم"
        },
        {
            "id": 5357,
            "verse_key": "69:34",
            "text_uthmani_simple": "ولا يحض على طعام المسكين"
        },
        {
            "id": 5358,
            "verse_key": "69:35",
            "text_uthmani_simple": "فليس له اليوم ههنا حميم"
        },
        {
            "id": 5359,
            "verse_key": "69:36",
            "text_uthmani_simple": "ولا طعام الا من غسلين"
        },
        {
            "id": 5360,
            "verse_key": "69:37",
            "text_uthmani_simple": "لا ياكله الا الخطـون"
        },
        {
            "id": 5361,
            "verse_key": "69:38",
            "text_uthmani_simple": "فلا اقسم بما تبصرون"
        },
        {
            "id": 5362,
            "verse_key": "69:39",
            "text_uthmani_simple": "وما لا تبصرون"
        },
        {
            "id": 5363,
            "verse_key": "69:40",
            "text_uthmani_simple": "انه لقول رسول كريم"
        },
        {
            "id": 5364,
            "verse_key": "69:41",
            "text_uthmani_simple": "وما هو بقول شاعر قليلا ما تؤمنون"
        },
        {
            "id": 5365,
            "verse_key": "69:42",
            "text_uthmani_simple": "ولا بقول كاهن قليلا ما تذكرون"
        },
        {
            "id": 5366,
            "verse_key": "69:43",
            "text_uthmani_simple": "تنزيل من رب العلمين"
        },
        {
            "id": 5367,
            "verse_key": "69:44",
            "text_uthmani_simple": "ولو تقول علينا بعض الاقاويل"
        },
        {
            "id": 5368,
            "verse_key": "69:45",
            "text_uthmani_simple": "لاخذنا منه باليمين"
        },
        {
            "id": 5369,
            "verse_key": "69:46",
            "text_uthmani_simple": "ثم لقطعنا منه الوتين"
        },
        {
            "id": 5370,
            "verse_key": "69:47",
            "text_uthmani_simple": "فما منكم من احد عنه حجزين"
        },
        {
            "id": 5371,
            "verse_key": "69:48",
            "text_uthmani_simple": "وانه لتذكرة للمتقين"
        },
        {
            "id": 5372,
            "verse_key": "69:49",
            "text_uthmani_simple": "وانا لنعلم ان منكم مكذبين"
        },
        {
            "id": 5373,
            "verse_key": "69:50",
            "text_uthmani_simple": "وانه لحسرة على الكفرين"
        },
        {
            "id": 5374,
            "verse_key": "69:51",
            "text_uthmani_simple": "وانه لحق اليقين"
        },
        {
            "id": 5375,
            "verse_key": "69:52",
            "text_uthmani_simple": "فسبح باسم ربك العظيم"
        }
    ],
