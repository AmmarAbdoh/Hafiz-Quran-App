{
    "verses": [
        {
            "id": 6024,
            "verse_key": "90:1",
            "text_uthmani_simple": "لا اقسم بهذا البلد"
        },
        {
            "id": 6025,
            "verse_key": "90:2",
            "text_uthmani_simple": "وانت حلۢ بهذا البلد"
        },
        {
            "id": 6026,
            "verse_key": "90:3",
            "text_uthmani_simple": "ووالد وما ولد"
        },
        {
            "id": 6027,
            "verse_key": "90:4",
            "text_uthmani_simple": "لقد خلقنا الانسن فى كبد"
        },
        {
            "id": 6028,
            "verse_key": "90:5",
            "text_uthmani_simple": "ايحسب ان لن يقدر عليه احد"
        },
        {
            "id": 6029,
            "verse_key": "90:6",
            "text_uthmani_simple": "يقول اهلكت مالا لبدا"
        },
        {
            "id": 6030,
            "verse_key": "90:7",
            "text_uthmani_simple": "ايحسب ان لم يره احد"
        },
        {
            "id": 6031,
            "verse_key": "90:8",
            "text_uthmani_simple": "الم نجعل له عينين"
        },
        {
            "id": 6032,
            "verse_key": "90:9",
            "text_uthmani_simple": "ولسانا وشفتين"
        },
        {
            "id": 6033,
            "verse_key": "90:10",
            "text_uthmani_simple": "وهدينه النجدين"
        },
        {
            "id": 6034,
            "verse_key": "90:11",
            "text_uthmani_simple": "فلا اقتحم العقبة"
        },
        {
            "id": 6035,
            "verse_key": "90:12",
            "text_uthmani_simple": "وما ادرىك ما العقبة"
        },
        {
            "id": 6036,
            "verse_key": "90:13",
            "text_uthmani_simple": "فك رقبة"
        },
        {
            "id": 6037,
            "verse_key": "90:14",
            "text_uthmani_simple": "او اطعم فى يوم ذى مسغبة"
        },
        {
            "id": 6038,
            "verse_key": "90:15",
            "text_uthmani_simple": "يتيما ذا مقربة"
        },
        {
            "id": 6039,
            "verse_key": "90:16",
            "text_uthmani_simple": "او مسكينا ذا متربة"
        },
        {
            "id": 6040,
            "verse_key": "90:17",
            "text_uthmani_simple": "ثم كان من الذين ءامنوا۟ وتواصوا۟ بالصبر وتواصوا۟ بالمرحمة"
        },
        {
            "id": 6041,
            "verse_key": "90:18",
            "text_uthmani_simple": "او۟لئك اصحب الميمنة"
        },
        {
            "id": 6042,
            "verse_key": "90:19",
            "text_uthmani_simple": "والذين كفروا۟ بـايتنا هم اصحب المشـمة"
        },
        {
            "id": 6043,
            "verse_key": "90:20",
            "text_uthmani_simple": "عليهم نار مؤصدةۢ"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "90"
        }
    }
}