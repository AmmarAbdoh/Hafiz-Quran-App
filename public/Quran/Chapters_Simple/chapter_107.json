{
    "verses": [
        {
            "id": 6198,
            "verse_key": "107:1",
            "text_uthmani_simple": "ارءيت الذى يكذب بالدين"
        },
        {
            "id": 6199,
            "verse_key": "107:2",
            "text_uthmani_simple": "فذلك الذى يدع اليتيم"
        },
        {
            "id": 6200,
            "verse_key": "107:3",
            "text_uthmani_simple": "ولا يحض على طعام المسكين"
        },
        {
            "id": 6201,
            "verse_key": "107:4",
            "text_uthmani_simple": "فويل للمصلين"
        },
        {
            "id": 6202,
            "verse_key": "107:5",
            "text_uthmani_simple": "الذين هم عن صلاتهم ساهون"
        },
        {
            "id": 6203,
            "verse_key": "107:6",
            "text_uthmani_simple": "الذين هم يراءون"
        },
        {
            "id": 6204,
            "verse_key": "107:7",
            "text_uthmani_simple": "ويمنعون الماعون"
        }
    ],
