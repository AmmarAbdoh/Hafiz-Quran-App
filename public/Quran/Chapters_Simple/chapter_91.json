{
    "verses": [
        {
            "id": 6044,
            "verse_key": "91:1",
            "text_uthmani_simple": "والشمس وضحىها"
        },
        {
            "id": 6045,
            "verse_key": "91:2",
            "text_uthmani_simple": "والقمر اذا تلىها"
        },
        {
            "id": 6046,
            "verse_key": "91:3",
            "text_uthmani_simple": "والنهار اذا جلىها"
        },
        {
            "id": 6047,
            "verse_key": "91:4",
            "text_uthmani_simple": "واليل اذا يغشىها"
        },
        {
            "id": 6048,
            "verse_key": "91:5",
            "text_uthmani_simple": "والسماء وما بنىها"
        },
        {
            "id": 6049,
            "verse_key": "91:6",
            "text_uthmani_simple": "والارض وما طحىها"
        },
        {
            "id": 6050,
            "verse_key": "91:7",
            "text_uthmani_simple": "ونفس وما سوىها"
        },
        {
            "id": 6051,
            "verse_key": "91:8",
            "text_uthmani_simple": "فالهمها فجورها وتقوىها"
        },
        {
            "id": 6052,
            "verse_key": "91:9",
            "text_uthmani_simple": "قد افلح من زكىها"
        },
        {
            "id": 6053,
            "verse_key": "91:10",
            "text_uthmani_simple": "وقد خاب من دسىها"
        },
        {
            "id": 6054,
            "verse_key": "91:11",
            "text_uthmani_simple": "كذبت ثمود بطغوىها"
        },
        {
            "id": 6055,
            "verse_key": "91:12",
            "text_uthmani_simple": "اذ انۢبعث اشقىها"
        },
        {
            "id": 6056,
            "verse_key": "91:13",
            "text_uthmani_simple": "فقال لهم رسول الله ناقة الله وسقيها"
        },
        {
            "id": 6057,
            "verse_key": "91:14",
            "text_uthmani_simple": "فكذبوه فعقروها فدمدم عليهم ربهم بذنۢبهم فسوىها"
        },
        {
            "id": 6058,
            "verse_key": "91:15",
            "text_uthmani_simple": "ولا يخاف عقبها"
        }
    ],
    "meta": {
        "filters": {
            "chapter_number": "91"
        }
    }
}