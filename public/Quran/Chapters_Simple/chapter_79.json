{
    "verses": [
        {
            "id": 5713,
            "verse_key": "79:1",
            "text_uthmani_simple": "والنزعت غرقا"
        },
        {
            "id": 5714,
            "verse_key": "79:2",
            "text_uthmani_simple": "والنشطت نشطا"
        },
        {
            "id": 5715,
            "verse_key": "79:3",
            "text_uthmani_simple": "والسبحت سبحا"
        },
        {
            "id": 5716,
            "verse_key": "79:4",
            "text_uthmani_simple": "فالسبقت سبقا"
        },
        {
            "id": 5717,
            "verse_key": "79:5",
            "text_uthmani_simple": "فالمدبرت امرا"
        },
        {
            "id": 5718,
            "verse_key": "79:6",
            "text_uthmani_simple": "يوم ترجف الراجفة"
        },
        {
            "id": 5719,
            "verse_key": "79:7",
            "text_uthmani_simple": "تتبعها الرادفة"
        },
        {
            "id": 5720,
            "verse_key": "79:8",
            "text_uthmani_simple": "قلوب يومئذ واجفة"
        },
        {
            "id": 5721,
            "verse_key": "79:9",
            "text_uthmani_simple": "ابصرها خشعة"
        },
        {
            "id": 5722,
            "verse_key": "79:10",
            "text_uthmani_simple": "يقولون اءنا لمردودون فى الحافرة"
        },
        {
            "id": 5723,
            "verse_key": "79:11",
            "text_uthmani_simple": "اءذا كنا عظما نخرة"
        },
        {
            "id": 5724,
            "verse_key": "79:12",
            "text_uthmani_simple": "قالوا۟ تلك اذا كرة خاسرة"
        },
        {
            "id": 5725,
            "verse_key": "79:13",
            "text_uthmani_simple": "فانما هى زجرة وحدة"
        },
        {
            "id": 5726,
            "verse_key": "79:14",
            "text_uthmani_simple": "فاذا هم بالساهرة"
        },
        {
            "id": 5727,
            "verse_key": "79:15",
            "text_uthmani_simple": "هل اتىك حديث موسى"
        },
        {
            "id": 5728,
            "verse_key": "79:16",
            "text_uthmani_simple": "اذ نادىه ربه بالواد المقدس طوى"
        },
        {
            "id": 5729,
            "verse_key": "79:17",
            "text_uthmani_simple": "اذهب الى فرعون انه طغى"
        },
        {
            "id": 5730,
            "verse_key": "79:18",
            "text_uthmani_simple": "فقل هل لك الى ان تزكى"
        },
        {
            "id": 5731,
            "verse_key": "79:19",
            "text_uthmani_simple": "واهديك الى ربك فتخشى"
        },
        {
            "id": 5732,
            "verse_key": "79:20",
            "text_uthmani_simple": "فارىه الءاية الكبرى"
        },
        {
            "id": 5733,
            "verse_key": "79:21",
            "text_uthmani_simple": "فكذب وعصى"
        },
        {
            "id": 5734,
            "verse_key": "79:22",
            "text_uthmani_simple": "ثم ادبر يسعى"
        },
        {
            "id": 5735,
            "verse_key": "79:23",
            "text_uthmani_simple": "فحشر فنادى"
        },
        {
            "id": 5736,
            "verse_key": "79:24",
            "text_uthmani_simple": "فقال انا۠ ربكم الاعلى"
        },
        {
            "id": 5737,
            "verse_key": "79:25",
            "text_uthmani_simple": "فاخذه الله نكال الءاخرة والاولى"
        },
        {
            "id": 5738,
            "verse_key": "79:26",
            "text_uthmani_simple": "ان فى ذلك لعبرة لمن يخشى"
        },
        {
            "id": 5739,
            "verse_key": "79:27",
            "text_uthmani_simple": "ءانتم اشد خلقا ام السماء بنىها"
        },
        {
            "id": 5740,
            "verse_key": "79:28",
            "text_uthmani_simple": "رفع سمكها فسوىها"
        },
        {
            "id": 5741,
            "verse_key": "79:29",
            "text_uthmani_simple": "واغطش ليلها واخرج ضحىها"
        },
        {
            "id": 5742,
            "verse_key": "79:30",
            "text_uthmani_simple": "والارض بعد ذلك دحىها"
        },
        {
            "id": 5743,
            "verse_key": "79:31",
            "text_uthmani_simple": "اخرج منها ماءها ومرعىها"
        },
        {
            "id": 5744,
            "verse_key": "79:32",
            "text_uthmani_simple": "والجبال ارسىها"
        },
        {
            "id": 5745,
            "verse_key": "79:33",
            "text_uthmani_simple": "متعا لكم ولانعمكم"
        },
        {
            "id": 5746,
            "verse_key": "79:34",
            "text_uthmani_simple": "فاذا جاءت الطامة الكبرى"
        },
        {
            "id": 5747,
            "verse_key": "79:35",
            "text_uthmani_simple": "يوم يتذكر الانسن ما سعى"
        },
        {
            "id": 5748,
            "verse_key": "79:36",
            "text_uthmani_simple": "وبرزت الجحيم لمن يرى"
        },
        {
            "id": 5749,
            "verse_key": "79:37",
            "text_uthmani_simple": "فاما من طغى"
        },
        {
            "id": 5750,
            "verse_key": "79:38",
            "text_uthmani_simple": "وءاثر الحيوة الدنيا"
        },
        {
            "id": 5751,
            "verse_key": "79:39",
            "text_uthmani_simple": "فان الجحيم هى الماوى"
        },
        {
            "id": 5752,
            "verse_key": "79:40",
            "text_uthmani_simple": "واما من خاف مقام ربه ونهى النفس عن الهوى"
        },
        {
            "id": 5753,
            "verse_key": "79:41",
            "text_uthmani_simple": "فان الجنة هى الماوى"
        },
        {
            "id": 5754,
            "verse_key": "79:42",
            "text_uthmani_simple": "يسـلونك عن الساعة ايان مرسىها"
        },
        {
            "id": 5755,
            "verse_key": "79:43",
            "text_uthmani_simple": "فيم انت من ذكرىها"
        },
        {
            "id": 5756,
            "verse_key": "79:44",
            "text_uthmani_simple": "الى ربك منتهىها"
        },
        {
            "id": 5757,
            "verse_key": "79:45",
            "text_uthmani_simple": "انما انت منذر من يخشىها"
        },
        {
            "id": 5758,
            "verse_key": "79:46",
            "text_uthmani_simple": "كانهم يوم يرونها لم يلبثوا۟ الا عشية او ضحىها"
        }
    ],
